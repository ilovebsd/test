package com.acromate.dto.office;


public class WorkDeptDTO {

    private String subscriberID = "";
    private String deptID = "";
    private int idType = 0;
    private String subOrgID = "";
    private String name = "";
    private String deptName = "";
    private String subOrgName = "";
    private String position = "";
    private String duty = "";
    private String changeTime = "";

    public String    getSubscriberID() { return this.subscriberID; }
    public void   setSubscriberID(String subscriberID ) { this.subscriberID = subscriberID; }

    public String getDeptID() {return this.deptID; }
    public void   setDeptID(String deptID ) { this.deptID = deptID; }

    public int    getIdType() { return this.idType; }
    public void   setIdType(int idType ) { this.idType = idType; }

    public String getSubOrgID() { return this.subOrgID; }
    public void   setSubOrgID(String subOrgID ) { this.subOrgID = subOrgID; }

    public String getName() { return this.name; }
    public void   setName(String name ) { this.name = name; }

    public String getDeptName() { return this.deptName; }
    public void   setDeptName(String deptName ) { this.deptName = deptName; }

    public String getSubOrgName() { return this.subOrgName; }
    public void   setSubOrgName(String subOrgName ) { this.subOrgName = subOrgName; }

    public String getPosition() { return this.position; }
    public void   setPosition(String position ) { this.position = position; }

    public String getDuty() { return this.duty; }
    public void   setDuty(String duty ) { this.duty = duty; }

    public String getChangeTime() { return this.changeTime; }
    public void   setChangeTime(String changeTime ) { this.changeTime = changeTime; }


    public String toString(){

        StringBuffer sb = new StringBuffer("");
        sb.append("(subscriberID:" 	+ getSubscriberID() + ") ") ;
        sb.append("[deptID:"  	        + getDeptID() + "] ") ;
        sb.append("[idType:"      	+ getIdType() + "] ") ;
        sb.append("[subOrgID:"  	+ getSubOrgID() + "] ") ;
        sb.append("[name:"  	        + getName() + "] ") ;
        sb.append("[deptName:"  	+ getDeptName() + "] ") ;
        sb.append("[subOrgName:"  	+ getSubOrgName() + "] ") ;
        sb.append("[position:"  	+ getPosition() + "] ") ;
        sb.append("[duty:"  	        + getDuty() + "] ") ;
        sb.append("[changeTime:"  	+ getChangeTime() + "] ") ;

        return sb.toString();
        }
}
