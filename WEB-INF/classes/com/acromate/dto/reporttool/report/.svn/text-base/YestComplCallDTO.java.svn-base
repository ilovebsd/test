package com.acromate.dto.reporttool.report;

import com.acromate.data.DataIO;

public class YestComplCallDTO extends DataIO {
	int yestAttemptedCall;
	int yestComplCall;
	int lastWeekAttemptedCall;
	int hour;
	int min;

	public int getYestAttemptedCall() {
		return yestAttemptedCall;
	}
	public void setYestAttemptedCall(int yestAttemptedCall) {
		this.yestAttemptedCall = yestAttemptedCall;
	}

	public int getYestComplCall() {
		return yestComplCall;
	}
	public void setYestComplCall(int yestComplCall) {
		this.yestComplCall = yestComplCall;
	}

	public int getLastWeekAttemptedCall() {
		return lastWeekAttemptedCall;
	}
	public void setLastWeekAttemptedCall(int lastWeekAttemptedCall) {
		this.lastWeekAttemptedCall = lastWeekAttemptedCall;
	}

	public int getHour() {
		return hour;
	}
	public void setHour(int hour) {
		this.hour = hour;
	}

	public int getMin() {
		return min;
	}
	public void setMin(int min) {
		this.min = min;
	}

	@Override
	protected int getFieldData(String field, Index idx, StringBuilder value) {
		int errorcode = 0;
        boolean resultVal = false;
        StringBuilder fldAlias;
        if(field != null && !field.equals("")) resultVal = true;
        if(field == null) {
            fldAlias = new StringBuilder(); fldAlias.append("");
        } else {
           fldAlias  = new StringBuilder(); fldAlias.append(field);
        }

        if(compareFieldID(fldAlias, "yestAttemptedCall", idx, value) == true) {
            errorcode = getInteger(yestAttemptedCall, value);
            if(resultVal || errorcode < 0) return errorcode;
        }
        if(compareFieldID(fldAlias, "yestComplCall", idx, value) == true) {
            errorcode = getInteger(yestComplCall, value);
            if(resultVal || errorcode < 0) return errorcode;
        }
        if(compareFieldID(fldAlias, "lastWeekAttemptedCall", idx, value) == true) {
            errorcode = getInteger(lastWeekAttemptedCall, value);
            if(resultVal || errorcode < 0) return errorcode;
        }
        if(compareFieldID(fldAlias, "hour", idx, value) == true) {
            errorcode = getInteger(hour, value);
            if(resultVal || errorcode < 0) return errorcode;
        }
        if(compareFieldID(fldAlias, "min", idx, value) == true) {
            errorcode = getInteger(min, value);
            if(resultVal || errorcode < 0) return errorcode;
        }
		return (0);
	}
	@Override
	protected int setFieldData(String field, String value) {
        StringBuilder temp = new StringBuilder(field);
        if(compareFieldID(temp, "yestAttemptedCall") == true) {
            if(!value.equals("")) yestAttemptedCall = Integer.parseInt(value);
            return 0;
        }
        if(compareFieldID(temp, "yestComplCall") == true) {
            if(!value.equals("")) yestComplCall = Integer.parseInt(value);
            return 0;
        }
        if(compareFieldID(temp, "lastWeekAttemptedCall") == true) {
            if(!value.equals("")) lastWeekAttemptedCall = Integer.parseInt(value);
            return 0;
        }
        if(compareFieldID(temp, "hour") == true) {
            if(!value.equals("")) hour = Integer.parseInt(value);
            return 0;
        }
        if(compareFieldID(temp, "min") == true) {
            if(!value.equals("")) min = Integer.parseInt(value);
            return 0;
        }
		return (-1);
	}
}
