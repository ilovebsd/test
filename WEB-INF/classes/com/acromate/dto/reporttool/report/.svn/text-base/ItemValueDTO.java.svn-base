package com.acromate.dto.reporttool.report;

import com.acromate.data.DataIO;

public class ItemValueDTO extends DataIO {
    public final static int ITEM_STRING = 0;
    public final static int ITEM_GRAPH = 1;
    public final static int ITEM_LIST = 2;

    String systemName;
    int itemType = ITEM_STRING;
    String value="";
    YestComplCallListDTO yestComplCallList = new YestComplCallListDTO();
    SubItemListDTO subItemList = new SubItemListDTO();

    public void setSystemName(String systemName) {
        this.systemName = systemName;
    }

    public String getSystemName() {
        return systemName;
    }

    public void setItemType(int itemType) {
        this.itemType = itemType;
    }

    public int getItemType() {
        return itemType;
    }

    public void setYestComplCallList(YestComplCallListDTO yestComplCallList) {
        this.yestComplCallList = yestComplCallList;
    }

    public YestComplCallListDTO getYestComplCallList() {
        return yestComplCallList;
    }

    public void setValue(String value) {
        this.value = value;
    }

    public String getValue() {
        return value;
    }

    public void setSubItemList(SubItemListDTO subItemList) {
        this.subItemList = subItemList;
    }

    public SubItemListDTO getSubItemList() {
        return subItemList;
    }

    @Override
    protected int getFieldData(String field, Index idx,
                                       StringBuilder value) {
        int errorcode = 0;
        boolean resultVal = false;
        StringBuilder fldAlias;
        if (field != null && !field.equals("")) {
            resultVal = true;
        }
        if (field == null) {
            fldAlias = new StringBuilder();
            fldAlias.append("");
        } else {
            fldAlias = new StringBuilder();
            fldAlias.append(field);
        }

        if (compareFieldID(fldAlias, "systemName", idx, value) == true) {
            errorcode = getString(systemName.toString(), value, !resultVal);
            if (resultVal || errorcode < 0) {
                return errorcode;
            }
        }
        if(compareFieldID(fldAlias, "itemType", idx, value) == true) {
            errorcode = getInteger(itemType, value);
            if(resultVal || errorcode < 0) return errorcode;
        }
        if (compareFieldID(fldAlias, "value", idx, value) == true) {
            errorcode = getString(this.value.toString(), value, !resultVal);
            if (resultVal || errorcode < 0) {
                return errorcode;
            }
        }

        if (compareFieldID(fldAlias, "yestComplCallList", idx, value) == true) {
            errorcode = yestComplCallList.getParam(fldAlias.toString(), value);
            if (resultVal || errorcode < 0) {
                return errorcode;
            }
        }
        if (compareFieldID(fldAlias, "subItemList", idx, value) == true) {
            errorcode = subItemList.getParam(fldAlias.toString(), value);
            if (resultVal || errorcode < 0) {
                return errorcode;
            }
        }
        return (0);
    }

    @Override
    protected int setFieldData(String field, String value) {
        StringBuilder temp = new StringBuilder(field);
        if (compareFieldID(temp, "systemName") == true) {
            if (!value.equals("")) {
                systemName = value;
            }
            return 0;
        }
        if(compareFieldID(temp, "itemType") == true) {
            if(value.equals("0")) ;
            else if(!value.equals("")) itemType = Integer.valueOf(value);
            return 0;
        }
        if (compareFieldID(temp, "value") == true) {
            if (!value.equals("")) {
                this.value = value;
            }
            return 0;
        }
        if (compareFieldID(temp, "yestComplCallList") == true) {
            yestComplCallList.removeAll();
            return yestComplCallList.setParam(temp.toString(), value);
        }
        if (compareFieldID(temp, "subItemList") == true) {
            subItemList.removeAll();
            return subItemList.setParam(temp.toString(), value);
        }
        return ( -1);

    }
}
