package com.acromate.dto.reporttool.report;

import com.acromate.data.DataIO;

public class SubItemDTO extends DataIO {
	String item;
	String value;

	public void setItem(String item) { this.item = item; }
	public  String getItem() { return item; }

	public void setValue(String value) { this.value = value; }
	public  String getValue() { return value; }
	@Override
	protected int getFieldData(String field, Index idx, StringBuilder value) {
		int errorcode = 0;
        boolean resultVal = false;
        StringBuilder fldAlias;
        if(field != null && !field.equals("")) resultVal = true;
        if(field == null) {
            fldAlias = new StringBuilder(); fldAlias.append("");
        } else {
           fldAlias  = new StringBuilder(); fldAlias.append(field);
        }

        if(compareFieldID(fldAlias, "item", idx, value) == true) {
            errorcode = getString(item.toString(), value, !resultVal);
            if(resultVal || errorcode < 0) return errorcode;
        }
        if(compareFieldID(fldAlias, "value", idx, value) == true) {
            errorcode = getString(this.value.toString(), value, !resultVal);
            if(resultVal || errorcode < 0) return errorcode;
        }
        return(0) ;
	}
	@Override
	protected int setFieldData(String field, String value) {
        StringBuilder temp = new StringBuilder(field);
        if(compareFieldID(temp, "item") == true) {
            if(!value.equals("")) item = value;
            return 0;
        }
        if(compareFieldID(temp, "value") == true) {
            if(value.equals("0")) ;
            else if(!value.equals("")) this.value = value;
            return 0;
        }
        return (-1);
	}
}
