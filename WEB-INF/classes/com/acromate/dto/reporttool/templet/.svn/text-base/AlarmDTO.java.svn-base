package com.acromate.dto.reporttool.templet;

import com.acromate.data.DataIO;

public class AlarmDTO  extends DataIO {
	boolean currentAlarm;
	boolean yestAlarm;

	public void setCurrentAlarm(boolean currentAlarm) { this.currentAlarm = currentAlarm; }
	public boolean getCurrentAlarm() { return currentAlarm; }

	public void setYestAlarm(boolean yestAlarm) { this.yestAlarm = yestAlarm; }
	public boolean getYestAlarm() { return yestAlarm; }

	protected int getFieldData(String field, Index idx, StringBuilder value) {
		int errorcode = 0;
        boolean resultVal = false;
        StringBuilder fldAlias;
        if(field != null && !field.equals("")) resultVal = true;
        if(field == null) {
            fldAlias = new StringBuilder(); fldAlias.append("");
        } else {
           fldAlias  = new StringBuilder(); fldAlias.append(field);
        }

        if(compareFieldID(fldAlias, "currentAlarm", idx, value) == true) {
            errorcode = getBoolean(currentAlarm, value);
            if(resultVal || errorcode < 0) return errorcode;
        }
        if(compareFieldID(fldAlias, "yestAlarm", idx, value) == true) {
            errorcode = getBoolean(yestAlarm, value);
            if(resultVal || errorcode < 0) return errorcode;
        }
		return (0);
	}

	protected int setFieldData(String field, String value) {
        StringBuilder temp = new StringBuilder(field);
        if(compareFieldID(temp, "currentAlarm") == true) {
            if(!value.equals("")) currentAlarm = Boolean.valueOf(value).booleanValue();
            return 0;
        }
        if(compareFieldID(temp, "yestAlarm") == true) {
            if(!value.equals("")) yestAlarm = Boolean.valueOf(value).booleanValue();
            return 0;
        }
		return (-1);
	}
}
