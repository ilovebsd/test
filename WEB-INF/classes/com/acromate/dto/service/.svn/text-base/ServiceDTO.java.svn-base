package com.acromate.dto.service ;

import com.acromate.lang.l;
import com.acromate.data.code.ServiceConst;

public class ServiceDTO
{

    public final static int MENU_TYPE_UPDATE = 1 ;
    public final static int MENU_TYPE_DETAIL = 0 ;

    private String e164 = "";
    private int    No = 0 ;
    private String Name = "";
    //private String FieldName = l.x("전화번호", "NUMBER");
    private String FieldName = l.x("입력값", "Input value");
    private int    Enter = 0 ;
    private int    Active = 0 ;
    private boolean    Popup = false ;
    private Object    PopupData = null ;
    private int menuType = MENU_TYPE_UPDATE ;

    public String getE164() { return this.e164; }
    public void   setE164(String e164 ) { this.e164 = e164; }

    public int    getNo() { return this.No; }
    public void   setNo(int No ) { this.No = No; }

    public String getName() { return this.Name; }
    public void   setName(String Name ) { this.Name = Name; }

    public String getFieldName() {
        switch ( this.No ) {
            case ServiceConst.CALLER_VCID :
            case ServiceConst.CALLER_HOT_LINE :
            case ServiceConst.ANSWER_CALL_FORWARD_DIRECT :
            case ServiceConst.ANSWER_CALL_FORWARD_BUSY :
            case ServiceConst.ANSWER_CALL_FORWARD_NOANSWER :
            case ServiceConst.ANSWER_CALL_FORWARD_TERM :
            case ServiceConst.RCF_OUT_MEMBER :
                return l.x("전화번호", "NUMBER");
            case ServiceConst.MRBT :
            case ServiceConst.MOH :
                return l.x("음원파일", "Sound File");
            default : return l.x("입력값", "Input value");
        }
    }
    public void   setFieldName(String FieldName ) { this.FieldName = FieldName; }

    public int    getEnter() { return this.Enter ; }
    public void   setEnter( int Enter ) { this.Enter = Enter ; }

    public int getActive() { return this.Active; }
    public void   setActive(int Active ) { this.Active = Active; }

    public boolean getPopup() { return this.Popup; }
    public void   setPopup(boolean Popup ) { this.Popup = Popup; }

    public Object getPopupData() { return this.PopupData; }
    public void   setPopupData( Object PopupData ) { this.PopupData = PopupData; }

    public int getMenuType() { return this.menuType; }
    public void   setMenuType( int menuType ) { this.menuType = menuType; }


}

