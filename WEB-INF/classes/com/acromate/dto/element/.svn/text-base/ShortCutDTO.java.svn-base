public class ShortCutDTO
{
    public final static int NO_SERVICE = 0 ;
    public final static int COMMON = 1 ;
    public final static int COMMON_PLUS_PRIVATE = 2 ;

    private String e164;
    private String shortCutNumber;
    private String targetNumber;
    private String description;

    public String getE164() { return this.e164; }
    public void   setE164(String vE164 ) { this.e164 = vE164; }

    public String getShortCutNumber() { return this.shortCutNumber; }
    public void   setShortCutNumber(String vShortCutNumber ) { this.shortCutNumber = vShortCutNumber; }

    public String getTargetNumber() { return this.targetNumber; }
    public void   setTargetNumber(String vTargetNumber ) { this.targetNumber = vTargetNumber; }

    public String getDescription() { return this.description; }
    public void   setDescription(String vDescription ) { this.description = vDescription; }


    public String toString()
    {
        StringBuffer sb = new StringBuffer("");

        sb.append("(e164:"                            + getE164() + ") ") ;
        sb.append("[shortCutNumber:"                  + getShortCutNumber() + "] ") ;
        sb.append("[targetNumber:"                    + getTargetNumber() + "] ") ;
        sb.append("[description:"                     + getDescription() + "] ") ;

        return sb.toString();
    }
}


