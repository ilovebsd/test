public class PresenceDTO
{
    private String userE164;
    private String endpointID;
    private int    protocol;
    private int    userType;
    private int    options;
    private int    registerTime;
    private int    presenceStatus;

    public String getUserE164() { return this.userE164; }
    public void   setUserE164(String vUserE164 ) { this.userE164 = vUserE164; }

    public String getEndpointID() { return this.endpointID; }
    public void   setEndpointID(String vEndpointID ) { this.endpointID = vEndpointID; }

    public int    getProtocol() { return this.protocol; }
    public void   setProtocol(int vProtocol ) { this.protocol = vProtocol; }

    public int    getUserType() { return this.userType; }
    public void   setUserType(int vUserType ) { this.userType = vUserType; }

    public int    getOptions() { return this.options; }
    public void   setOptions(int vOptions ) { this.options = vOptions; }

    public int    getRegisterTime() { return this.registerTime; }
    public void   setRegisterTime(int vRegisterTime ) { this.registerTime = vRegisterTime; }

    public int    getPresenceStatus() { return this.presenceStatus; }
    public void   setPresenceStatus(int vPresenceStatus ) { this.presenceStatus = vPresenceStatus; }

    public String toString()
    {
        StringBuffer sb = new StringBuffer("");

        sb.append("(userE164:"                  + getUserE164() + ") ") ;
        sb.append("[endpointID:"                + getEndpointID() + "] ") ;
        sb.append("[protocol:"                  + getProtocol() + "] ") ;
        sb.append("[userType:"                  + getUserType() + "] ") ;
        sb.append("[options:"                   + getOptions() + "] ") ;
        sb.append("[registerTime:"              + getRegisterTime() + "] ") ;
        sb.append("[presenceStatus:"            + getPresenceStatus() + "] ") ;

        return sb.toString();
    }
}
