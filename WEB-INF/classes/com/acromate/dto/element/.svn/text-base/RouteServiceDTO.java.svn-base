public class RouteServiceDTO
{

    private String endpointID;
    private int    protocol;
    private int    serviceNo;
    private int    priority;
    private String userParam;
    private int    inoutFlag;
    private String serviceName;
    private int    serviceClass;

    public String getEndpointID() { return this.endpointID; }
    public void   setEndpointID(String vEndpointID ) { this.endpointID = vEndpointID; }

    public int    getProtocol() { return this.protocol; }
    public void   setProtocol(int vProtocol ) { this.protocol = vProtocol; }

    public int    getServiceNo() { return this.serviceNo; }
    public void   setServiceNo(int vServiceNo ) { this.serviceNo = vServiceNo; }

    public int    getPriority() { return this.priority; }
    public void   setPriority(int vPriority ) { this.priority = vPriority; }

    public String getUserParam() { return this.userParam; }
    public void   setUserParam(String vUserParam ) { this.userParam = vUserParam; }

    public int    getInoutFlag() { return this.inoutFlag; }
    public void   setInoutFlag(int vInoutFlag ) { this.inoutFlag = vInoutFlag; }

    /*************************************************************************/
    public String getServiceName() { return this.serviceName; }
    public void   setServiceName(String vServiceName ) { this.serviceName = vServiceName; }
    /*************************************************************************/

    public int    getServiceClass() { return this.serviceClass; }
    public void   setServiceClass(int vServiceClass ) { this.serviceClass = vServiceClass; }

    public String toString()
    {
        StringBuffer sb = new StringBuffer("");

        sb.append("(endpointID:"                  + getEndpointID() + ") ") ;
        sb.append("[protocol:"                    + getProtocol() + "] ") ;
        sb.append("[serviceNo:"                   + getServiceNo() + "] ") ;
        sb.append("[priority:"                    + getPriority() + "] ") ;
        sb.append("[userParam:"                   + getUserParam() + "] ") ;
        sb.append("[inoutFlag:"                   + getInoutFlag() + "] ") ;
        sb.append("[serviceName:"                 + getServiceName() + "] ") ;

        return sb.toString();
    }
}


