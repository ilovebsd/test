public class PrefixTableIDDTO
{


    private String prefixTableID;
    private int    tableType;
    private String description;

    public String getPrefixTableID() { return this.prefixTableID; }
    public void   setPrefixTableID(String vPrefixTableID ) { this.prefixTableID = vPrefixTableID; }

    public int    getTableType() { return this.tableType; }
    public void   setTableType(int vTableType ) { this.tableType = vTableType; }

    public String getDescription() { return this.description; }
    public void   setDescription(String vDescription ) { this.description = vDescription; }


    public String toString()
    {
        StringBuffer sb = new StringBuffer("");

        sb.append("(prefixTableID:"              + getPrefixTableID() + ") ") ;
        sb.append("[tableType:"                  + getTableType() + "] ") ;
        sb.append("[description:"                + getDescription() + "] ") ;

        return sb.toString();
    }
}
