public class E164RouteDTO
{

    private String routingNumber = "";
    private String e164 = "";
    private String endpointID= "";
    private int    protocol;
    private int    endpointType;
    private int    routingNumberType;
    private int    priority ;

    public String getRoutingNumber() { return this.routingNumber; }
    public void   setRoutingNumber(String vRoutingNumber) { this.routingNumber = vRoutingNumber; }

    public String getE164() { return this.e164; }
    public void   setE164(String vE164 ) { this.e164 = vE164; }

    public String getEndpointID() { return this.endpointID; }
    public void   setEndpointID(String vEndpointID ) { this.endpointID = vEndpointID; }

    public int    getProtocol() { return this.protocol; }
    public void   setProtocol(int vProtocol ) { this.protocol = vProtocol; }

    public int    getEndpointType() { return this.endpointType; }
    public void   setEndpointType(int vEndpointType ) { this.endpointType = vEndpointType; }

    public int    getRoutingNumberType() { return this.routingNumberType; }
    public void   setRoutingNumberType(int vRoutingNumberType ) { this.routingNumberType = vRoutingNumberType; }

    public int    getPriority() { return this.priority; }
    public void   setPriority(int vPriority ) { this.priority = vPriority; }

    public String toString()
    {
        StringBuffer sb = new StringBuffer("");

        sb.append("(routingNumber:"               + getRoutingNumber() + ") ") ;
        sb.append("[e164:"                        + getE164() + "] ") ;
        sb.append("[endpointID:"                  + getEndpointID() + "] ") ;
        sb.append("[protocol:"                    + getProtocol() + "] ") ;
        sb.append("[endpointType:"                + getEndpointType() + "] ") ;
        sb.append("[routingNumberType:"           + getRoutingNumberType() + "] ") ;
        sb.append("[priority:"                    + getPriority() + "] ") ;

        return sb.toString();
    }
}
