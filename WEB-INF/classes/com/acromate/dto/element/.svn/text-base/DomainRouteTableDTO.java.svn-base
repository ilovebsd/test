public class DomainRouteTableDTO
{

    private String domainID;
    private int    routeSelectRule;
    private String routeID;
    private int    protocol;
    private int    callType;
    private int    chargeType;
    private String description;
    private int    routingEndpointType;
    private int    endpointClass;

    public String getDomainID() { return this.domainID; }
    public void   setDomainID(String vDomainID ) { this.domainID = vDomainID; }

    public int    getRouteSelectRule() { return this.routeSelectRule; }
    public void   setRouteSelectRule(int vRouteSelectRule ) { this.routeSelectRule = vRouteSelectRule; }

    public String getRouteID() { return this.routeID; }
    public void   setRouteID(String vRouteID ) { this.routeID = vRouteID; }

    public int    getProtocol() { return this.protocol; }
    public void   setProtocol(int vProtocol ) { this.protocol = vProtocol; }

    public int    getCallType() { return this.callType; }
    public void   setCallType(int vCallType ) { this.callType = vCallType; }

    public int    getChargeType() { return this.chargeType; }
    public void   setChargeType(int vChargeType ) { this.chargeType = vChargeType; }

    public String getDescription() { return this.description; }
    public void   setDescription(String vDescription ) { this.description = vDescription; }

    public int    getRoutingEndpointType() { return this.routingEndpointType; }
    public void   setRoutingEndpointType(int vRoutingEndpointType ) { this.routingEndpointType = vRoutingEndpointType; }

    public int    getEndpointClass() { return this.endpointClass; }
    public void   setEndpointClass(int endpointClass ) { this.endpointClass = endpointClass; }


    public String toString()
    {
        StringBuffer sb = new StringBuffer("");

        sb.append("(domainID:"                  + getDomainID() + ") ") ;
        sb.append("[direction:"                 + getRouteSelectRule() + "] ") ;
        sb.append("[orderNumber:"               + getRouteID() + "] ") ;
        sb.append("[protocol:"                  + getProtocol() + "] ") ;
        sb.append("[callType:"                  + getCallType() + "] ") ;
        sb.append("[chargeType:"                + getChargeType() + "] ") ;
        sb.append("[description:"               + getDescription() + "] ") ;
        sb.append("[routingEndpointType:"       + getRoutingEndpointType() + "] ") ;
        return sb.toString();
    }
}


