public class RouteMonitorDTO
{

    private String endpointID;
    private int    protocol;
    private int    checkMode;
    private String fromNumber;
    private String toNumber;
    private int    messageType;
    private int    subType;
    private String subID;


    public String getEndpointID() { return this.endpointID; }
    public void   setEndpointID(String vEndpointID ) { this.endpointID = vEndpointID; }

    public int    getProtocol() { return this.protocol; }
    public void   setProtocol(int vProtocol ) { this.protocol = vProtocol; }

    public int    getCheckMode() { return this.checkMode; }
    public void   setCheckMode(int vCheckMode ) { this.checkMode = vCheckMode; }

    public String getFromNumber() { return this.fromNumber; }
    public void   setFromNumber(String vFromNumber ) { this.fromNumber = vFromNumber; }

    public String getToNumber() { return this.toNumber; }
    public void   setToNumber(String vToNumber ) { this.toNumber = vToNumber; }

    public int    getMessageType() { return this.messageType; }
    public void   setMessageType(int vMessageType ) { this.messageType = vMessageType; }

    public int    getSubType() { return this.subType; }
    public void   setSubType(int vSubType ) { this.subType = vSubType; }

    public String getSubID() { return this.subID; }
    public void   setSubID(String vSubID ) { this.subID = vSubID; }




    public String toString()
    {
        StringBuffer sb = new StringBuffer("");

        sb.append("(endpointID:"                  + getEndpointID() + ") ") ;
        sb.append("[protocol:"                    + getProtocol() + "] ") ;
        sb.append("[checkMode:"                   + getCheckMode() + "] ") ;
        sb.append("[fromNumber:"                  + getFromNumber() + "] ") ;
        sb.append("[toNumber:"                    + getToNumber() + "] ") ;
        sb.append("[messageType:"                 + getMessageType() + "] ") ;
        sb.append("[subType:"                     + getSubType() + "] ") ;
        sb.append("[subID:"                       + getSubID() + "] ") ;
        return sb.toString();
    }
}


