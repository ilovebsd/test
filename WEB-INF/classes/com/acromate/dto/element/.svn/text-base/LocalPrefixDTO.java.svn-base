public class LocalPrefixDTO
{

    public final static int PREFIX_TYPE_LOCAL_PREFIX    = 0x01 ;
    public final static int PREFIX_TYPE_SPECIAL_PREFIX    = 0x02 ;
    public final static int PREFIX_TYPE_NEIGHBOR_PREFIX    = 0x01 ;
    public final static int PREFIX_TYPE_KEY_NUMBER   = 0x04 ;

    private String startPrefix;
    private String endPrefix;
    private int    prefixType;
    private String endpointID;
    private int    protocol;
    private String groupID;
    private int    options;
    private int    maxDigit;

    public String getStartPrefix() { return this.startPrefix; }
    public void   setStartPrefix(String vStartPrefix ) { this.startPrefix = vStartPrefix; }

    public String getEndPrefix() { return this.endPrefix; }
    public void   setEndPrefix(String vEndPrefix ) { this.endPrefix = vEndPrefix; }

    public int    getPrefixType() { return this.prefixType; }
    public void   setPrefixType(int vPrefixType ) { this.prefixType = vPrefixType; }

    public String getEndpointID() { return this.endpointID; }
    public void   setEndpointID(String vEndpointID ) { this.endpointID = vEndpointID; }

    public int    getProtocol() { return this.protocol; }
    public void   setProtocol(int vProtocol ) { this.protocol = vProtocol; }

    public String getGroupID() { return this.groupID; }
    public void   setGroupID(String vGroupID ) { this.groupID = vGroupID; }

    public int getOptions() { return this.options; }
    public void   setOptions(int options ) { this.options = options; }

    public int getMaxDigit() { return this.maxDigit; }
    public void   setMaxDigit(int maxDigit ) { this.maxDigit = maxDigit; }


    public String toString()
    {
        StringBuffer sb = new StringBuffer("");

        sb.append("(startPrefix:"              + getStartPrefix() + ") ") ;
        sb.append("[endPrefix:"                + getEndPrefix() + "] ") ;
        sb.append("[prefixType:"               + getPrefixType() + "] ") ;
        sb.append("[endpointID:"               + getEndpointID() + "] ") ;
        sb.append("[protocol:"                 + getProtocol() + "] ") ;
        sb.append("[groupID:"                  + getGroupID() + "] ") ;
        sb.append("[options:"                  + getOptions() + "] ") ;
        sb.append("[maxDigit:"                  + getMaxDigit() + "] ") ;

        return sb.toString();
    }
}
