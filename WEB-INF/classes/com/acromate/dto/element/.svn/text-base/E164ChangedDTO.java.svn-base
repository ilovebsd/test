public class E164ChangedDTO
{

    public final static int INFO_MENT = 0;
    public final static int AUTO_CALL_FORWARD = 1;

    private String e164 = "" ;
    private String    newE164 = "" ;
    private String    changedTime = "" ;
    private String    period = "" ;
    private int    options = 0 ;

    public String getE164() { return this.e164; }
    public void   setE164(String vE164 ) { this.e164 = vE164; }

    public String    getNewE164() { return this.newE164; }
    public void   setNewE164(String vNewE164 ) { this.newE164 = vNewE164; }

    public String    getChangedTime() { return this.changedTime; }
    public void   setChangedTime(String vChangedTime ) { this.changedTime = vChangedTime; }

    public String    getPeriod() { return this.period; }
    public void   setPeriod(String period ) { this.period = period; }

    public int    getOptions() { return this.options; }
    public void   setOptions(int options ) { this.options = options; }

    public String toString()
    {
        StringBuffer sb = new StringBuffer("") ;

        sb.append("(e164:"                      + getE164() + ") ") ;
        sb.append("[newE164:"                   + getNewE164() + "] ") ;
        sb.append("[changedTime:"               + getChangedTime() + "] ") ;
        sb.append("[period:"                   + getPeriod() + "] ") ;
        sb.append("[options:"                   + getOptions() + "] ") ;

        return sb.toString() ;
    }
}


