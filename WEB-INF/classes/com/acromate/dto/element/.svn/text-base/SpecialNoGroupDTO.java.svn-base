public class SpecialNoGroupDTO
{

    private String specialNoGID;
    private String specialNo;
    private String prefixTableID;
    private int    emergencyYN;

    public String getSpecialNoGID() { return this.specialNoGID; }
    public void   setSpecialNoGID(String vSpecialNoGID ) { this.specialNoGID = vSpecialNoGID; }

    public String getSpecialNo() { return this.specialNo; }
    public void   setSpecialNo(String vSpecialNo ) { this.specialNo = vSpecialNo; }

    public String getPrefixTableID() { return this.prefixTableID; }
    public void   setPrefixTableID(String vPrefixTableID ) { this.prefixTableID = vPrefixTableID; }

    public int    getEmergencyYN() { return this.emergencyYN; }
    public void   setEmergencyYN(int vEmergencyYN ) { this.emergencyYN = vEmergencyYN; }



    public String toString()
    {
        StringBuffer sb = new StringBuffer("");

        sb.append("(specialNoGID:"              + getSpecialNoGID() + ") ") ;
        sb.append("[specialNo:"                 + getSpecialNo() + "] ") ;
        sb.append("[specialNo:"                 + getPrefixTableID() + "] ") ;
        sb.append("[emergencyYN:"               + getEmergencyYN() + "] ") ;

        return sb.toString();
    }
}
