public class CommonShortCutDTO
{

    private String serviceNumber;
    private int    serviceType;
    private String routingNumber;
    private String description;
    private String userParam1;
    private String userParam2;

    public String getServiceNumber() { return this.serviceNumber; }
    public void   setServiceNumber(String vServiceNumber ) { this.serviceNumber = vServiceNumber; }

    public int    getServiceType() { return this.serviceType; }
    public void   setServiceType(int vServiceType ) { this.serviceType = vServiceType; }

    public String getRoutingNumber() { return this.routingNumber; }
    public void   setRoutingNumber(String vRoutingNumber ) { this.routingNumber = vRoutingNumber; }

    public String getDescription() { return this.description; }
    public void   setDescription(String vDescription ) { this.description = vDescription; }

    public String getUserParam1() { return this.userParam1; }
    public void   setUserParam1(String vUserParam1 ) { this.userParam1 = vUserParam1; }

    public String getUserParam2() { return this.userParam2; }
    public void   setUserParam2(String vUserParam2 ) { this.userParam2 = vUserParam2; }


    public String toString()
    {
        StringBuffer sb = new StringBuffer("");

        sb.append("(serviceNumber:"               + getServiceNumber() + ") ") ;
        sb.append("[serviceType:"                 + getServiceType() + "] ") ;
        sb.append("[routingNumber:"               + getRoutingNumber() + "] ") ;
        sb.append("[description:"                 + getDescription() + "] ") ;
        sb.append("[userParam1:"                  + getUserParam1() + "] ") ;
        sb.append("[description:"                 + getUserParam2() + "] ") ;

        return sb.toString();
    }
}


