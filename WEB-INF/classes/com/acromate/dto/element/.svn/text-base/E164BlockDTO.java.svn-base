public class E164BlockDTO
{
/**
0 : 발신제한
1: 착신제한
*/
    public final static int BLOCK_CALLER_OUT = 0 ;
    public final static int BLOCK_ANSWER_IN = 1 ;

    private String e164 = "";
    private int    inoutFlag;
    private String startPrefix = "";
    private String endPrefix = "";
    private String blockID = "";
    private int    prefixType;
    private int    blockIDType;
    private String description = "";

    public String getE164() { return this.e164; }
    public void   setE164(String vE164 ) { this.e164 = vE164; }

    public int    getInoutFlag() { return this.inoutFlag; }
    public void   setInoutFlag(int vInoutFlag ) { this.inoutFlag = vInoutFlag; }

    public String getStartPrefix() { return this.startPrefix; }
    public void   setStartPrefix(String vStartPrefix ) { this.startPrefix = vStartPrefix; }

    public String getEndPrefix() { return this.endPrefix; }
    public void   setEndPrefix(String vEndPrefix ) { this.endPrefix = vEndPrefix; }

    public String getBlockID() { return this.blockID; }
    public void   setBlockID(String vBlockID ) { this.blockID = vBlockID; }

    public int    getPrefixType() { return this.prefixType; }
    public void   setPrefixType(int vPrefixType ) { this.prefixType = vPrefixType; }

    public int    getBlockIDType() { return this.blockIDType; }
    public void   setBlockIDType(int vBlockIDType ) { this.blockIDType = vBlockIDType; }

    public String getDescription() { return this.description; }
    public void   setDescription(String vDescription ) { this.description = vDescription; }





    public String toString()
    {
        StringBuffer sb = new StringBuffer("");

        sb.append("(e164:"                      + getE164() + ") ") ;
        sb.append("[inoutFlag:"                 + getInoutFlag() + "] ") ;
        sb.append("[startPrefix:"               + getStartPrefix() + "] ") ;
        sb.append("[endPrefix:"                 + getEndPrefix() + "] ") ;
        sb.append("[blockID:"                   + getBlockID() + "] ") ;
        sb.append("[prefixType:"                + getPrefixType() + "] ") ;
        sb.append("[blockIDType:"               + getBlockIDType() + "] ") ;
        sb.append("[description:"               + getDescription() + "] ") ;

        return sb.toString();
    }
}


