public class FeatureDetailDTO
{

    private int    serviceNo;
    private String serviceName;
    private int    serviceClass;
    private String endpointID;
    private int    endpointType;
    private int    inoutFlag;
    private int    service;
    private String param1;
    private String param2;
    private String param3;

    public int    getServiceNo() { return this.serviceNo; }
    public void   setServiceNo(int vServiceNo ) { this.serviceNo = vServiceNo; }

    public String getServiceName() { return this.serviceName; }
    public void   setServiceName(String vServiceName ) { this.serviceName = vServiceName; }

    public int    getServiceClass() { return this.serviceClass; }
    public void   setServiceClass(int vServiceClass ) { this.serviceClass = vServiceClass; }

    public String getEndpointID() { return this.endpointID; }
    public void   setEndpointID(String vEndpointID ) { this.endpointID = vEndpointID; }

    public int    getEndpointType() { return this.endpointType; }
    public void   setEndpointType(int vEndpointType ) { this.endpointType = vEndpointType; }

    public int    getInoutFlag() { return this.inoutFlag; }
    public void   setInoutFlag(int vInoutFlag ) { this.inoutFlag = vInoutFlag; }

    public int    getService() { return this.service; }
    public void   setService(int vService ) { this.service = vService; }

    public String getParam1() { return this.param1; }
    public void   setParam1(String vParam1 ) { this.param1 = vParam1; }

    public String getParam2() { return this.param2; }
    public void   setParam2(String vParam2 ) { this.param2 = vParam2; }

    public String getParam3() { return this.param3; }
    public void   setParam3(String vParam3 ) { this.param3 = vParam3; }



    public String toString()
    {
        StringBuffer sb = new StringBuffer("");

        sb.append("(serviceNo:"               + getServiceNo() + ") ") ;
        sb.append("[serviceName:"             + getServiceName() + "] ") ;
        sb.append("[serviceClass:"            + getServiceClass() + "] ") ;
        sb.append("[endpointID:"              + getEndpointID() + "] ") ;
        sb.append("[endpointType:"            + getEndpointType() + "] ") ;
        sb.append("[inoutFlag:"               + getInoutFlag() + "] ") ;
        sb.append("[service:"                 + getService() + "] ") ;
        sb.append("[param1:"                  + getParam1() + "] ") ;
        sb.append("[param2:"                  + getParam2() + "] ") ;
        sb.append("[param3:"                  + getParam3() + "] ") ;

        return sb.toString();
    }
}


