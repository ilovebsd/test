public class HuntingGroupDTO
{

    private String groupID;
    private String huntingGroupID;
    private String e164;
    private String forwardE164;
    private int    useHunting;
    private String description;
    private int    huntingOrder;

    public String getGroupID() { return this.groupID; }
    public void   setGroupID(String vGroupID ) { this.groupID = vGroupID; }

    public String getHuntingGroupID() { return this.huntingGroupID; }
    public void   setHuntingGroupID(String vHuntingGroupID ) { this.huntingGroupID = vHuntingGroupID; }

    public String getE164() { return this.e164; }
    public void   setE164(String vE164 ) { this.e164 = vE164; }

    public String getForwardE164() { return this.forwardE164; }
    public void   setForwardE164(String vForwardE164 ) { this.forwardE164 = vForwardE164; }

    public int    getUseHunting() { return this.useHunting; }
    public void   setUseHunting(int vUseHunting ) { this.useHunting = vUseHunting; }

    public String getDescription() { return this.description; }
    public void   setDescription(String vDescription ) { this.description = vDescription; }

    public int    getHuntingOrder() { return this.huntingOrder; }
    public void   setHuntingOrder(int vHuntingOrder ) { this.huntingOrder = vHuntingOrder; }


    public String toString()
    {
        StringBuffer sb = new StringBuffer("");

        sb.append("(groupID:"                  + getGroupID() + ") ") ;
        sb.append("[huntingGroupID:"           + getHuntingGroupID() + "] ") ;
        sb.append("[e164:"                     + getE164() + "] ") ;
        sb.append("[forwardE164:"              + getForwardE164() + "] ") ;
        sb.append("[useHunting:"               + getUseHunting() + "] ") ;
        sb.append("[description:"              + getDescription() + "] ") ;
        sb.append("[huntingOrder:"             + getHuntingOrder() + "] ") ;

        return sb.toString();
    }
}


