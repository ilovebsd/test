public class RouteGroupIDDTO
{

    private String	routegroupid;
    private int	        hunt;
    private int	        huntindex;
    private int	        maxhuntindex;
    private int	        maxdetourcount;
    private int         maxAvailableFaultCall;
    private int         outAddibleFlag;


/**
 * member functions
 */
public String	getRoutegroupid() { return this.routegroupid; }
public void	setRoutegroupid(String vRoutegroupid ) { this.routegroupid = vRoutegroupid; }

public int	getHunt() { return this.hunt; }
public void	setHunt(int vHunt ) { this.hunt = vHunt; }

public int	getHuntindex() { return this.huntindex; }
public void	setHuntindex(int vHuntindex ) { this.huntindex = vHuntindex; }

public int	getMaxhuntindex() { return this.maxhuntindex; }
public void	setMaxhuntindex(int vMaxhuntindex ) { this.maxhuntindex = vMaxhuntindex; }

public int	getMaxdetourcount() { return this.maxdetourcount; }
public void	setMaxdetourcount(int vMaxdetourcount ) { this.maxdetourcount = vMaxdetourcount; }

public int	getMaxAvailableFaultCall() { return this.maxAvailableFaultCall; }
public void	setMaxAvailableFaultCall(int vMaxAvailableFaultCall ) { this.maxAvailableFaultCall = vMaxAvailableFaultCall; }

public int	getOutAddibleFlag() { return this.outAddibleFlag; }
public void	setOutAddibleFlag(int vOutAddibleFlag ) { this.outAddibleFlag = vOutAddibleFlag; }


public String toString()
{
        StringBuffer sb = new StringBuffer("");

        sb.append("(routegroupid:" 	                + getRoutegroupid() + ") ") ;
        sb.append("[hunt:"    	                        + getHunt() + "] ") ;
        sb.append("[huntindex:"     	                + getHuntindex() + "] ") ;
        sb.append("[maxhuntindex:" 	                + getMaxhuntindex() + "] ") ;
        sb.append("[maxdetourcount:" 	                + getMaxdetourcount() + "] ") ;
        sb.append("[maxAvailableFaultCall:" 	        + getMaxAvailableFaultCall() + "] ") ;
        sb.append("[outAddibleFlag:" 	                + getOutAddibleFlag() + "] ") ;

         return sb.toString();
     }
 }
