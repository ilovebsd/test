public class MoveProxyListDTO {

    private String hostID  = "";
    private int moveType  = 0;
    private String hostIP  = "";
    private String descriptor = "";

    public String getHostID() {return this.hostID; }
    public void setHostID(String vHostID ) { this.hostID = vHostID; }

    public int getMoveType() {return this.moveType; }
    public void setMoveType(int vMoveType ) { this.moveType = vMoveType; }

    public String getHostIP() {return this.hostIP; }
    public void setHostIP(String vHostIP ) { this.hostIP = vHostIP; }

    public String getDescriptor() {return this.descriptor; }
    public void setDescriptor(String vDescriptor ) { this.descriptor = vDescriptor; }

    public String toString(){

        StringBuffer sb = new StringBuffer("");
        sb.append("(hostID:" 	        + getHostID() + ") ") ;
        sb.append("(moveType:" 	        + getMoveType() + ") ") ;
        sb.append("[hostIP:"  	        + getHostIP() + "] ") ;
        sb.append("[descriptor:"      	+ getDescriptor() + "] ") ;

        return sb.toString();
    }
}
