public class ConnectedCallDTO
{
    private String callID;
    private int    startTime;
    private String callerEndpointID;
    private int    callerEndpointClass;
    private String callerE164;
    private String callerEndpointIP;
    private String calleeEndpointID;
    private int    calleeEndpointClass;
    private String calleeE164;
    private String calleeEndpointIP;
    private int    callCnt;

    public String getCallID() { return this.callID; }
    public void   setCallID(String vCallID ) { this.callID = vCallID; }

    public int    getStartTime() { return this.startTime; }
    public void   setStartTime(int vStartTime ) { this.startTime = vStartTime; }

    public String getCallerEndpointID() { return this.callerEndpointID; }
    public void   setCallerEndpointID(String vCallerEndpointID ) { this.callerEndpointID = vCallerEndpointID; }

    public int    getCallerEndpointClass() { return this.callerEndpointClass; }
    public void   setCallerEndpointClass(int vCallerEndpointClass ) { this.callerEndpointClass = vCallerEndpointClass; }

    public String getCallerE164() { return this.callerE164; }
    public void   setCallerE164(String vCallerE164 ) { this.callerE164 = vCallerE164; }

    public String getCallerEndpointIP() { return this.callerEndpointIP; }
    public void   setCallerEndpointIP(String vCallerEndpointIP ) { this.callerEndpointIP = vCallerEndpointIP; }

    public String getCalleeEndpointID() { return this.calleeEndpointID; }
    public void   setCalleeEndpointID(String vCalleeEndpointID ) { this.calleeEndpointID = vCalleeEndpointID; }

    public int    getCalleeEndpointClass() { return this.calleeEndpointClass; }
    public void   setCalleeEndpointClass(int vCalleeEndpointClass ) { this.calleeEndpointClass = vCalleeEndpointClass; }

    public String getCalleeE164() { return this.calleeE164; }
    public void   setCalleeE164(String vCalleeE164 ) { this.calleeE164 = vCalleeE164; }

    public String getCalleeEndpointIP() { return this.calleeEndpointIP; }
    public void   setCalleeEndpointIP(String vCalleeEndpointIP ) { this.calleeEndpointIP = vCalleeEndpointIP; }

    public int    getCallCnt() { return this.callCnt; }
    public void   setCallCnt(int vCallCnt ) { this.callCnt = vCallCnt; }

    public String toString()
    {
        StringBuffer sb = new StringBuffer("");

        sb.append("(callID:"                 + getCallID() + ") ") ;
        sb.append("[startTime:"              + getStartTime() + "] ") ;
        sb.append("[callerEndpointID:"       + getCallerEndpointID() + "] ") ;
        sb.append("[callerEndpointClass:"    + getCallerEndpointClass() + "] ") ;
        sb.append("[callerE164:"             + getCallerE164() + "] ") ;
        sb.append("[callerEndpointIP:"       + getCallerEndpointIP() + "] ") ;
        sb.append("[calleeEndpointID:"       + getCalleeEndpointID() + "] ") ;
        sb.append("[calleeEndpointClass:"    + getCalleeEndpointClass() + "] ") ;
        sb.append("[calleeE164:"             + getCalleeE164() + "] ") ;
        sb.append("[calleeEndpointIP:"       + getCalleeEndpointIP() + "] ") ;
        sb.append("[callCnt:"                + getCallCnt() + "] ") ;

        return sb.toString();
    }
}
