public class CurrentAlarmDTO {

    private int	indexNo;
    private String sysGroupID;
    private String hwClusterID;
    private String hwID;
    private String clusterID;
    private String objectName;
    private String systemPath;
    private String objectPath;
    private int alarmType;
    private int alarmLevel;
    private String alarmMessage;
    private String checkTime;
    private String lastInTime;
    private int viewInfo;
    private String code;
    private String alarmName;
    private int autoCloseWait;


    public int    getIndexNo() { return this.indexNo; }
    public void   setIndexNo(int indexNo ) { this.indexNo = indexNo; }

    public String getSysGroupID() { return this.sysGroupID; }
    public void   setSysGroupID(String sysGroupID ) { this.sysGroupID = sysGroupID; }

    public String getHwClusterID() { return this.hwClusterID; }
    public void   setHwClusterID(String hwClusterID ) { this.hwClusterID = hwClusterID; }

    public String getHwID() { return this.hwID; }
    public void   setHwID(String hwID ) { this.hwID = hwID; }

    public String getClusterID() { return this.clusterID; }
    public void   setClusterID(String clusterID ) { this.clusterID = clusterID; }

    public String getObjectName() { return this.objectName; }
    public void   setObjectName(String objectName ) { this.objectName = objectName; }

    public String getSystemPath() { return this.systemPath; }
    public void   setSystemPath(String systemPath ) { this.systemPath = systemPath; }

    public String getObjectPath() { return this.objectPath; }
    public void   setObjectPath(String objectPath ) { this.objectPath = objectPath; }

    public int    getAlarmType() { return this.alarmType; }
    public void   setAlarmType(int alarmType ) { this.alarmType = alarmType; }

    public int    getAlarmLevel() { return this.alarmLevel; }
    public void   setAlarmLevel(int alarmLevel ) { this.alarmLevel = alarmLevel; }

    public String getAlarmMessage() { return this.alarmMessage; }
    public void   setAlarmMessage(String alarmMessage ) { this.alarmMessage = alarmMessage; }

    public String getCheckTime() { return this.checkTime; }
    public void   setCheckTime(String checkTime ) { this.checkTime = checkTime; }

    public String getLastInTime() { return this.lastInTime; }
    public void   setLastInTime(String lastInTime ) { this.lastInTime = lastInTime; }

    public int    getViewInfo() { return this.viewInfo; }
    public void   setViewInfo(int viewInfo ) { this.viewInfo = viewInfo; }

    public String getCode() { return this.code; }
    public void   setCode(String code ) { this.code = code; }

    public String getAlarmName() { return this.alarmName; }
    public void   setAlarmName(String alarmName ) { this.alarmName = alarmName; }

    public int    getAutoCloseWait() { return this.autoCloseWait; }
    public void   setAutoCloseWait(int autoCloseWait ) { this.autoCloseWait = autoCloseWait; }




    public String toString()
    {
        StringBuffer sb = new StringBuffer("");

        sb.append("(indexNo:"                       + getIndexNo() + ") ") ;
        sb.append("[sysGroupID:"                    + getSysGroupID() + "] ") ;
        sb.append("[hwClusterID:"                   + getHwClusterID() + "] ") ;
        sb.append("[hwID:"                          + getHwID() + "] ") ;
        sb.append("[clusterID:"                     + getClusterID() + "] ") ;
        sb.append("[objectName:"                    + getObjectName() + "] ") ;
        sb.append("[systemPath:"                    + getSystemPath() + "] ") ;
        sb.append("[objectPath:"                    + getObjectPath() + "] ") ;
        sb.append("[alarmType:"                     + getAlarmType() + "] ") ;
        sb.append("[alarmLevel:"                    + getAlarmLevel() + "] ") ;
        sb.append("[alarmMessage:"                  + getAlarmMessage() + "] ") ;
        sb.append("[checkTime:"                     + getCheckTime() + "] ") ;
        sb.append("[lastInTime:"                    + getLastInTime() + "] ") ;
        sb.append("[viewInfo:"                      + getViewInfo() + "] ") ;
        sb.append("[code:"                          + getCode() + "] ") ;
        sb.append("[alarmName:"                     + getAlarmName() + "] ") ;
        sb.append("[autoCloseWait:"                 + getAutoCloseWait() + "] ") ;
        return sb.toString();
    }


}
