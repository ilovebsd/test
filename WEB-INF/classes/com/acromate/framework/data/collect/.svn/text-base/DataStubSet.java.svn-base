package com.acromate.framework.data.collect;

import java.util.Hashtable;

/**
 *
 * @author 아크로메이트 기술연구소
 * @version 3.0.0
 */
public class DataStubSet extends Hashtable<Object, Object> {
	
 	public DataStubSet() {        
    }

    /**
     * 
     * @return 
     */
    public int getElementCount() {
        return this.size();
    }
    
    /**
     * 
     * @param element 
     */
    public void addElement(Object key, Object element) {        
        this.put(key, element);
    }

    /**
     * 
     * @param element 
     */
    public void removeElement(Object key, Object element) {
        if(this.containsKey(key)) {
            this.remove(key);
        }
    }

    /** 
     * 
     */
    public void removeAll() {
        if(this.size() > 0) {
            this.clear();
        }
    }

    /**
     * 
     * @param element 
     * @return 
     */
    public Object findElement(Object key) {
        Object object = null;
        object = this.get(key);
        if(object != null) {
            return object;
        }   return object;
    }
}
