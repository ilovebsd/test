package com.acromate.framework;

/**
 *
 * @author 아크로메이트 기술연구소
 */
public abstract class BaseObject {
    private String version = "0";
    public BaseObject() {
    }

     /**
     *
     * @return
     */

    public String getVersion() {
        return version;
    }

    /**
     *
     * @param version
     */

    public void setVersion(String version) {
        this.version = version;
    }
}
