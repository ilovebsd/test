package com.acromate.dataio.server.alarm;

/**
 * <p>Title:CheckObject </p>
 *
 * <p>Description : 특정 Object 의 장애 임계치 설정 정보  객체</p>
*
 * <p>Copyright: Copyright by Chun Jungho(c) 2007</p>
 *
 * <p>Company:Acromate </p>
 *
 * params
 * SysGroupID : 해당 그룹아이디
 * ClusterID : 해당 장치 아이디
 * SystemName : 아래 SystemType에 따른 ID
 * ObjectPath  : 체크할 객체의 정보 - Script 로 경로 및 객체표시
 * ObjectName : 장애이름
 * SystemType : 1 : Hw ID ,  2 : Hw Cluster ID, 3 : Process ID,  4 : Process Cluster ID
 * CompareType : 0 : 범위 - Critical, Major, Minor 조건에 해당하면 장애발생
 *                                     -  클수록 장애 정도가 심함.
 *                         1 : 범위 - Critical, Major, Minor  조건에 해당하면 장애발생
 *                                      -  작을수록 장애 정도가 심함.
 *                         2 : T/F - False 시 장애발생
 *
 * CriticalValue : Critical 장애 임계값
 * MajorValue : Major 장애 임계값
 * MinorValue : Minor 장애 임계값
 * Description : 설명
 */
public class CheckObject {
    private String strSysGroupID;
    private String strClusterID;
    private String strHwID;
    private String strSystemPath;
    private String strObjectPath;
    private String strObjectName;
    private int clusterType;
    private int nAlarmType;
    private int nCompareType;
    private int nCriticalValue;
    private int nMajorValue;
    private int nMinorValue;
    private String strDescription;
    private String strMessage = "";

    public final static int COMPARE_TYPE_BIGGER =  0 ;    /* 클수록 장애 정도가 심함    */
    public final static int COMPARE_TYPE_SMALLER =  1 ;      /* 작을수록 장애 정도가 심함 */
    public final static int COMPARE_TYPE_BOOLEAN =  2 ; /* T/F - False 시 장애발생    */

    public CheckObject( String SysGroupID, String ClusterID, String strHwID, String strSystemPath, String ObjectPath,
		String ObjectName, int clusterType, int nAlarmType, int CompareType,
		int CriticalValue, int MajorValue, int MinorValue, String Description) {
        this.strSysGroupID = SysGroupID;
        this.strHwID = strHwID;
        this.strClusterID = ClusterID;
        this.strSystemPath = strSystemPath;
        this.strObjectPath = ObjectPath;
        this.strObjectName = ObjectName;
        this.clusterType = clusterType;
        this.nAlarmType = nAlarmType;
        this.nCompareType = CompareType;
        this.nCriticalValue = CriticalValue;
        this.nMajorValue = MajorValue;
        this.nMinorValue = MinorValue;
        this.strDescription = Description;
    }

    public String getSysGroupID() { return strSysGroupID; }
    public void setSysGroupID(String value) { this.strSysGroupID = value; }
    public String getClusterID(){  return strClusterID;  }
    public void setClusterID(String value){  this.strClusterID = value;  }
    public String getstrHwID(){  return strHwID;  }
    public void setstrHwID(String value){  this.strHwID = value;  }
    public String getObjectPath(){  return strObjectPath;  }
    public void setObjectPath(String value){  this.strObjectPath = value;  }
    public String getSystemPath(){  return strSystemPath;  }
    public void setSystemPath(String value){  this.strSystemPath = value;  }
    public String getObjectName(){  return strObjectName;  }
    public void setObjectName(String value){  this.strObjectName = value;  }
    public int getClusterType(){  return clusterType;  }
    public void setClusterType(int  value){  this. clusterType = value;  }
    public int getAlarmType(){  return nAlarmType;  }
    public void setAlarmType(int  value){  this. nAlarmType = value;  }
    public int getCompareType(){  return nCompareType;  }
    public void setCompareType(int  value){  this. nCompareType = value;  }
    public int getCriticalValue(){  return nCriticalValue;  }
    public void setCriticalValue(int  value){  this. nCriticalValue = value;  }
    public int getMajorValue(){  return nMajorValue;  }
    public void setMajorValue(int  value){  this. nMajorValue = value;  }
    public int getMinorValue(){  return nMinorValue;  }
    public void setMinorValue(int  value){  this. nMinorValue  = value ;  }
    public String getDescription(){  return strDescription;  }
    public void setDescription(String value){  this.strDescription  = value ;  }
    public String getMessage(){  return strMessage;  }
    public void setMessage(String value){  this.strMessage  = value ;  }
}

