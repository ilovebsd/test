public class AlarmObject {
    private String strSysGroupID;
    private String strClusterID;
    private String strHwClusterID;
    private String strHwID;
    private String strObjectName;
    private String strSystemPath;
    private String strObjectPath;

    private int nAlarmType;
    private int nAlarmLevel;
    private int nMessageType;  // Alarm Fault Info

    private String strAlarmMessage;
    private int nViewInfo = 1;
    private String strAlarmCode;
    private String strAlarmName;
    private String strCheckTime;
    private int nAutoCloseWait;

    public AlarmObject(){}

    public String getSysGroupID() { return strSysGroupID; }
    public void setSysGroupID(String value) { this.strSysGroupID = value; }
    public String getHwClusterID() { return strHwClusterID; }
    public void setHwClusterID(String value) { this.strHwClusterID = value; }
    public String getClusterID(){  return strClusterID;  }
    public void setClusterID(String value){  this.strClusterID = value;  }
    public String getstrHwID(){  return strHwID;  }
    public void setstrHwID(String value){  this.strHwID = value;  }
    public String getObjectPath(){  return strObjectPath;  }
    public void setObjectPath(String value){  this.strObjectPath = value;  }
    public String getSystemPath(){  return strSystemPath;  }
    public void setSystemPath(String value){  this.strSystemPath = value;  }
    public String getObjectName(){  return strObjectName;  }
    public void setObjectName(String value){  this.strObjectName = value;  }

    public int getMessageType(){  return nMessageType;  }
    public void setMessageType(int  value){  this. nMessageType = value;  }
	
    public int getAlarmType(){  return nAlarmType;  }
    public void setAlarmType(int  value){  this. nAlarmType = value;  }
    public int getAlarmLevel(){  return nAlarmLevel;  }
    public void setAlarmLevel(int  value){  this. nAlarmLevel  = value ;  }

    public int getViewInfo(){  return nViewInfo;  }
    public void setViewInfo(int  value){  this. nViewInfo = value;  }

    public String getAlarmMessage(){  return strAlarmMessage;  }
    public void setAlarmMessage(String value){  this.strAlarmMessage  = value ;  }

    public String getAlarmCode(){  return strAlarmCode;  }
    public void setAlarmCode(String value){  this.strAlarmCode  = value ;  }
    public String getAlarmName(){  return strAlarmName;  }
    public void setAlarmName(String value){  this.strAlarmName  = value ;  }

    public String getCheckTime(){  return strCheckTime;  }
    public void setCheckTime(String value){  this.strCheckTime  = value ;  }

    public int getAutoCloseWait(){  return nAutoCloseWait;  }
    public void setAutoCloseWait(int value){  this.nAutoCloseWait  = value ;  }
    public String toString(){
        StringBuffer sb = new StringBuffer("");
        sb.append("[strSysGroupID:" 	+getSysGroupID() + "] ") ;
        sb.append("[strClusterID:" 	 	+getClusterID() + "] ") ;
        sb.append("[strHwClusterID:" 	+getHwClusterID() + "] ") ;
        sb.append("[strHwID:" 	+getstrHwID() + "] ") ;
        sb.append("[strObjectName:" 	+getObjectName() + "] ") ;
        sb.append("[strSystemPath:" 	+getSystemPath() + "] ") ;
        sb.append("[strObjectPath:" 	+getObjectPath() + "] ") ;

        sb.append("[AlarmType:" 	 	+getAlarmType() + "] ") ;
        sb.append("[AlarmLevel:" 	 	+getAlarmLevel() + "] ") ;
        sb.append("[MessageType:" 	+getMessageType() + "] ") ; 

        sb.append("[AlarmMessage:" 	+getAlarmMessage() + "] ") ;
        sb.append("[ViewInfo:" 	 	+getViewInfo() + "] ") ;
        sb.append("[AlarmCode:" 	+getAlarmCode() + "] ") ;
        sb.append("[AlarmName:" 	+getAlarmName() + "] ") ;
        sb.append("[CheckTime:" 	+getCheckTime() + "] ") ;
        sb.append("[AutoCloseWait:" 	+getAutoCloseWait() + "] ") ;
        return sb.toString();
    }
}

