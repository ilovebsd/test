package AcroRestore;

import java.io.IOException;
import java.util.ArrayList;

import framework.zip.Uncompress;

public class RestoreUnZip {
	public Uncompress uncompress	=	new Uncompress();
	public int Execut(String scrFileFullName,String outPutDir){
		int nReturn	=	0;
		try {
			uncompress.unzip(scrFileFullName, outPutDir);
			nReturn=1;
		} catch (IOException e) {
			e.printStackTrace();
		}
		return nReturn;
	}
	public ArrayList getArrayList(){
		return uncompress.alistFilenName;
	}
}
