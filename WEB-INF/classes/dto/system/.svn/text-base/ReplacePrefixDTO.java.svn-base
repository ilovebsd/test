package dto.system;

public class ReplacePrefixDTO {
	/**
	 * member variables
	 */
	private int		seqindex;
	private int		condition;
	private String	conditiondigit;
	private String	updatedigit;

	public ReplacePrefixDTO() {      
		super();
	}

	public ReplacePrefixDTO(int seqindex, int condition, String conditiondigit, String updatedigit) {      
		
		super();
		
		this.seqindex 		= seqindex;
		this.condition 		= condition;
		this.conditiondigit = conditiondigit;
		this.updatedigit 	= updatedigit;
	}
	
	/**
	 * member functions
	 */
	public int	getSeqindex() { return this.seqindex; }
	public void	setSeqindex(int seqindex ) { this.seqindex = seqindex; }

	public int	getCondition() { return this.condition; }
	public void	setCondition(int condition ) { this.condition = condition; }

	public String	getConditiondigit() { return this.conditiondigit; }
	public void	setConditiondigit(String conditiondigit ) { this.conditiondigit = conditiondigit; }

	public String	getUpdatedigit() { return this.updatedigit; }
	public void	setUpdatedigit(String updatedigit ) { this.updatedigit = updatedigit; }

}