package dto.system;

public class SystemInfoDTO {
	/**
	 * member variables
	 */
	// Table_RouteState
	private String	endpointid;
	private int		stateFlag;
	private int		serviceFlag;
	private int		incurrentcall;
	private int		outcurrentcall;
	private int     timeoutfault;
	private int     regitime;
	private int     serverexpireTime;
	
	public SystemInfoDTO() {}
	
	public SystemInfoDTO(String endpointid, int stateFlag, int serviceFlag, int incurrentcall, int outcurrentcall, int timeoutfault, int regitime, int serverexpireTime) {      
		
		super();
		
		this.endpointid 		= endpointid;
		this.stateFlag 			= stateFlag;
		this.serviceFlag 		= serviceFlag;
		this.incurrentcall 		= incurrentcall;
		this.outcurrentcall 	= outcurrentcall;
		this.timeoutfault 		= timeoutfault;
		this.regitime 			= regitime;
		this.serverexpireTime 	= serverexpireTime;
	}

	/**
	 * Table_RouteState member functions
	 */	
	public String	getEndpointid() { return this.endpointid; }
	public void	setEndpointid(String endpointid ) { this.endpointid = endpointid; }

	public int	getStateFlag() { return this.stateFlag; }
	public void	setStateFlag(int StateFlag ) { this.stateFlag = StateFlag; }

	public int	getServiceFlag() { return this.serviceFlag; }
	public void	setServiceFlag(int serviceFlag ) { this.serviceFlag = serviceFlag; }

	public int	getInCurrentCall() { return this.incurrentcall; }
	public void	setInCurrentCall(int incurrentcall ) { this.incurrentcall = incurrentcall; }

	public int	getOutCurrentCall() { return this.outcurrentcall; }
	public void	setOutCurrentCall(int outcurrentcall ) { this.outcurrentcall = outcurrentcall; }

	public int	getTimeoutfault() { return this.timeoutfault; }
	public void	setTimeoutfault(int timeoutfault ) { this.timeoutfault = timeoutfault; }

	public int	getRegiTime() { return this.regitime; }
	public void	setRegiTime(int regitime ) { this.regitime = regitime; }

	public int	getServerExpireTime() { return this.serverexpireTime; }
	public void	setServerExpireTime(int serverexpireTime ) { this.serverexpireTime = serverexpireTime; }
	
}