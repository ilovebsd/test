package dto.system;

public class RouteMonitorDTO {
	/**
	 * member variables
	 */
	private String	endpointid;
	private int		protocol;
	private int		checkmode;
	private String  fromnumber;
	private String  tonumber;
	private int		messagetype;
	private int		subtype;
	private String	subid;	

	public RouteMonitorDTO() {      		
		super();
	}
	
	public RouteMonitorDTO(String endpointid, String subid, int checkmode, String fromnumber, String tonumber, int messagetype) {      		
		
		super();
		
		this.endpointid 	= endpointid;
		this.subid 			= subid;
		this.checkmode 		= checkmode;
		this.fromnumber 	= fromnumber;
		this.tonumber 		= tonumber;		
		this.messagetype 	= messagetype;		
	}
	
	/**
	 * member functions
	 */
	public String	getEndpointid() { return this.endpointid; }
	public void	setEndpointid(String endpointid ) { this.endpointid = endpointid; }

	public int	getProtocol() { return this.protocol; }
	public void	setProtocol(int protocol ) { this.protocol = protocol; }

	public int	getCheckmode() { return this.checkmode; }
	public void	setCheckmode(int checkmode ) { this.checkmode = checkmode; }
	
	public String	getFromnumber() { return this.fromnumber; }
	public void	setFromnumber(String fromnumber ) { this.fromnumber = fromnumber; }

	public String	getTonumber() { return this.tonumber; }
	public void	setTonumber(String tonumber ) { this.tonumber = tonumber; }

	public int	getMessagetype() { return this.messagetype; }
	public void	setMessagetype(int messagetype ) { this.messagetype = messagetype; }

	public int	getSubtype() { return this.subtype; }
	public void	setSubtype(int subtype ) { this.subtype = subtype; }
	
	public String	getSubid() { return this.subid; }
	public void	setSubid(String subid ) { this.subid = subid; }

}