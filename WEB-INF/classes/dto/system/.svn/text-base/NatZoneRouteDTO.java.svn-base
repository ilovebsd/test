package dto.system;

public class NatZoneRouteDTO {
	/**
	 * member variables
	 */
	private String	inzone;
	private String	outzone;
	private int		serviceflag;
	private int		trtype;
	private String	trid;
	
	public NatZoneRouteDTO() {      		
		
	}
	
	public NatZoneRouteDTO(String inzone, String outzone, int serviceflag, int trtype, String trid) {      		
		
		super();
		
		this.inzone 		= inzone;
		this.outzone 		= outzone;
		this.serviceflag 	= serviceflag;
		this.trtype 		= trtype;
		this.trid 			= trid;
	}

	/**
	 * member functions
	 */
	public String	getInzone() { return this.inzone; }
	public void	setInzone(String inzone ) { this.inzone = inzone; }

	public String	getOutzone() { return this.outzone; }
	public void	setOutzone(String outzone ) { this.outzone = outzone; }

	public int	getServiceflag() { return this.serviceflag; }
	public void	setServiceflag(int serviceflag ) { this.serviceflag = serviceflag; }

	public int	getTrtype() { return this.trtype; }
	public void	setTrtype(int trtype ) { this.trtype = trtype; }
	
	public String	getTrid() { return this.trid; }
	public void	setTrid(String trid ) { this.trid = trid; }

}