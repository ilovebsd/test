package dto.system;

public class RootErrBackUpDTO {
	/**
	 * member variables
	 */
	// Table_NeighborProxy
	private String	endpointid;
	private String	indomainname;
	private String	signaladdress;
	private int		backUpCount;
	private int		routeIndex;
	
	// Table_RouteState
	private String	routecode;
	
	public RootErrBackUpDTO() {}
	
	public RootErrBackUpDTO(String endpointid, String routecode, String indomainname, String signaladdress, int backUpCount, int routeIndex) {      
		
		super();
		
		this.endpointid 		= endpointid;
		this.routecode 			= routecode;
		this.indomainname 		= indomainname;
		this.signaladdress 		= signaladdress;
		this.backUpCount 		= backUpCount;
		this.routeIndex			= routeIndex;
	}
	
	/**
	 * Table_NeighborProxy member functions
	 */
	public String	getEndpointid() { return this.endpointid; }
	public void	setEndpointid(String endpointid ) { this.endpointid = endpointid; }

	public String	getInDomainname() { return this.indomainname; }
	public void	setInDomainname(String indomainname ) { this.indomainname = indomainname; }

	public String	getSignalAddress() { return this.signaladdress; }
	public void	setSignalAddress(String signaladdress ) { this.signaladdress = signaladdress; }

	
	/**
	 * Table_RouteState member functions
	 */	
	public String	getRouteCode() { return this.routecode; }
	public void	setRouteCode(String routecode ) { this.routecode = routecode; }

	
	
	public int	getBackUpCount() { return this.backUpCount; }
	public void	setBackUpCount(int backUpCount ) { this.backUpCount = backUpCount; }

	public int	getRouteIndex() { return this.routeIndex; }
	public void	setRouteIndex(int routeIndex ) { this.routeIndex = routeIndex; }
	
}