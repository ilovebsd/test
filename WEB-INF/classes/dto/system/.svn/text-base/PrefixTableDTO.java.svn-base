package dto.system;

public class PrefixTableDTO {
	/**
	 * member variables
	 */
	private String	prefixTableId;	
	private String	startPrefix;
	private String	endPrefix;
	private int		routeSelectRule;
	private String	routeId;
	private int		protocol;
	private int		callType;
	private int		chargeType;
	private String	description;	
	private int		minDigitCount;
	private int		maxDigitCount;
	private int		routingEndpointType;
	
	public PrefixTableDTO() {      
		super();
	}

	public PrefixTableDTO(String prefixtableid, String startprefix, String endprefix, int minDigitCount, int calltype, String description) {      
		
		super();
		
		this.prefixTableId 	= prefixtableid;
		this.startPrefix	= startprefix;
		this.endPrefix		= endprefix;
		this.minDigitCount	= minDigitCount;
		this.callType		= calltype;
		this.description 	= description;
	}

	public PrefixTableDTO(String prefixtableid, String startprefix, String endprefix, int minDigitCount, int maxDigitCount, int calltype, int routeSelectRule, String description, String routeId) {      
		
		super();
		
		this.prefixTableId 	= prefixtableid;
		this.startPrefix	= startprefix;
		this.endPrefix		= endprefix;
		this.minDigitCount	= minDigitCount;
		this.maxDigitCount	= maxDigitCount;
		this.callType		= calltype;
		this.description 	= description;
		this.routeId 		= routeId;
		this.routeSelectRule= routeSelectRule;
	}
	
	/**
	 * member functions
	 */
	public String	getPrefixTableId() { return this.prefixTableId; }
	public void	setPrefixTableId(String prefixTableId ) { this.prefixTableId = prefixTableId; }

	public String	getStartPrefix() { return this.startPrefix; }
	public void	setStartPrefix(String startPrefix ) { this.startPrefix = startPrefix; }
	
	public String	getEndPrefix() { return this.endPrefix; }
	public void	setEndPrefix(String vEndprefix ) { 
        //끝 Prefix에 '_'넣기
		endPrefix = "________________________________";
        if(vEndprefix.length() < 32) {
            int nLength = vEndprefix.length();
            endPrefix = vEndprefix + endPrefix.substring(nLength);
        }
        else if (vEndprefix.length() == 32 )
        	endPrefix = vEndprefix ;
        //System.out.println("Endprefix = " + endPrefix);		
	}
	
	public int	getRouteSelectRule() { return this.routeSelectRule; }
	public void	setRouteSelectRule(int routeSelectRule ) { this.routeSelectRule = routeSelectRule; }

	public String	getRouteId() { return this.routeId; }
	public void	setRouteId(String routeId ) { this.routeId = routeId; }

	public int	getProtocol() { return this.protocol; }
	public void	setProtocol(int protocol ) { this.protocol = protocol; }

	public int	getCallType() { return this.callType; }
	public void	setCallType(int callType ) { this.callType = callType; }

	public int	getChargeType() { return this.chargeType; }
	public void	setChargeType(int chargeType ) { this.chargeType = chargeType; }
		
	public String	getDescription() { return this.description; }
	public void	setDescription(String description ) { this.description = description; }

	public int	getMinDigitCount() { return this.minDigitCount; }
	public void	setMinDigitCount(int minDigitCount ) { this.minDigitCount = minDigitCount; }

	public int	getMaxDigitCount() { return this.maxDigitCount; }
	public void	setMaxDigitCount(int maxDigitCount ) { this.maxDigitCount = maxDigitCount; }

	public int	getRoutingEndpointType() { return this.routingEndpointType; }
	public void	setRoutingEndpointType(int routingEndpointType ) { this.routingEndpointType = routingEndpointType; }
	
}