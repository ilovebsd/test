package dto.ipcs;

public class IpcsVirtualDeptDTO {
	
	private String 	endpointid ;
	private String 	keynumberdesc;
	
	private String 	startprefix;
	private String 	endprefix;
	private int 	prefixtype;
	private int 	protocol;
	private String 	groupid;
	
	private String 	keynumberid;
	private int 	hunt;
	private int 	huntindex;
	private int 	maxhuntindex;
	private int 	huntconstraint;
	private String 	k_endpointid;
	private int 	k_protocol;
	private String 	k_sound;
	private int 	queueoption;
	private String 	queuestartann;
	private String 	queueendann;
	private int 	e164Count;
	
	public IpcsVirtualDeptDTO() {}

	public IpcsVirtualDeptDTO(String endpointid,String keynumberdesc,int hunt,String queuestartann,String queueendann,int e164Count) {      			
		super();
		
		this.endpointid 	= endpointid;
		this.keynumberdesc 	= keynumberdesc;
		this.hunt 			= hunt;
		this.queuestartann	= queuestartann;
		this.queueendann	= queueendann;
		this.e164Count		= e164Count;
	}

	public String getEndpointid() {
		return endpointid;
	}
	public void setEndpointid(String endpointid) {
		this.endpointid = endpointid;
	}
	
	public String getKeynumberdesc() {
		return keynumberdesc;
	}
	public void setKeynumberdesc(String keynumberdesc) {
		this.keynumberdesc = keynumberdesc;
	}
	
	// Table_Localprefix
    public String	getStartprefix() { 
    	return this.startprefix;
    }
    public void	setStartprefix(String vStartprefix ) { 
    	this.startprefix = vStartprefix; 
    }

    public String	getEndprefix() { 
    	return this.endprefix; 
    }
    public void	setEndprefix(String vEndprefix ) {
        //끝 Prefix에 '_'넣기
        endprefix = "________________________________";
        if(vEndprefix.length() < 32) {
            int nLength = vEndprefix.length();
            endprefix = vEndprefix + endprefix.substring(nLength);
        }
        else if (vEndprefix.length() == 32 )
            endprefix = vEndprefix ;
        System.out.println("Endprefix = " + endprefix);
    }

    public int	getPrefixtype() { 
    	return this.prefixtype; 
    }
    public void	setPrefixtype(int vPrefixtype ) { 
    	this.prefixtype = vPrefixtype; 
    }

    public int	getProtocol() { 
    	return this.protocol; 
    }
    public void	setProtocol(int vProtocol ) { 
    	this.protocol = vProtocol; 
    }

    public String	getGroupid() { 
    	return this.groupid; 
    }
    public void	setGroupid(String vGroupid ) { 
    	this.groupid = vGroupid; 
    }
	
    // Table_Keynumberid
	public String getKeynumberid() {
		return keynumberid;
	}
	public void setKeynumberid(String keynumberid) {
		this.keynumberid = keynumberid;
	}
    
    public int	getHunt() { 
    	return this.hunt; 
    }
    public void	setHunt(int hunt ) { 
    	this.hunt = hunt; 
    }
    
    public int	getHuntindex() { 
    	return this.huntindex; 
    }
    public void	setHuntindex(int huntindex ) { 
    	this.huntindex = huntindex; 
    }
    
    public int	getMaxhuntindex() { 
    	return this.maxhuntindex; 
    }
    public void	setMaxhuntindex(int maxhuntindex ) { 
    	this.maxhuntindex = maxhuntindex; 
    }
    
    public int	getHuntconstraint() { 
    	return this.huntconstraint; 
    }
    public void	setHuntconstraint(int huntconstraint ) { 
    	this.huntconstraint = huntconstraint; 
    }   
    
    
    // 2001.01.08 추가 Table_Keynumberid
	public String getK_endpointid() {
		return k_endpointid;
	}
	public void setK_endpointid(String k_endpointid) {
		this.k_endpointid = k_endpointid;
	}
    
    public int	getK_protocol() { 
    	return this.k_protocol; 
    }
    public void	setK_protocol(int k_protocol ) { 
    	this.k_protocol = k_protocol; 
    }

	public String getK_sound() {
		return k_sound;
	}
	public void setK_sound(String k_sound) {
		this.k_sound = k_sound;
	}

    public int	getQueueOption() { 
    	return this.queueoption; 
    }
    public void	setQueueOption(int queueoption ) { 
    	this.queueoption = queueoption; 
    }

	public String getQueueStartAnn() {
		return queuestartann;
	}
	public void setQueueStartAnn(String queuestartann) {
		this.queuestartann = queuestartann;
	}

	public String getQueueEndAnn() {
		return queueendann;
	}
	public void setQueueEndAnn(String queueendann) {
		this.queueendann = queueendann;
	}

    public int	getE164Count() { 
    	return this.e164Count; 
    }
    public void	setE164Count(int e164Count ) { 
    	this.e164Count = e164Count; 
    }

}
