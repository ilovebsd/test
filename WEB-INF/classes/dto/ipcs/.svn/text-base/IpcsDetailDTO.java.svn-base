package dto.ipcs;
 

public class IpcsDetailDTO {
	// Subscriber 테이블
	private String 	pwd;					// 비밀번호
	private String 	name;					// 이름
	private String 	position;				// 직급
	private int 	department;				// 부서
	private String 	mobile;					// 핸드폰
	private String 	homenumber;				// 집전화
	private String 	mailaddress;			// 메일주소
	private String 	extension;				// 내선번호
	
	private int 	authmode;				// 인증 Mode
	private String 	username;				// 인증 ID
	private String 	password;				// 인증비밀번호
	private String 	ipaddress;				// 인증 IP
	private int 	ipport;					// 인증 Port
	
	private String 	zonecode;				// ZoneCode
	private String 	beforePassword;			// 헥사 변경전 인증비밀번호
	
	public IpcsDetailDTO(String pwd,String name,String position,int department,String mobile,String homenumber,String mailaddress,int authmode,String username,String password,String ipaddress,int ipport,String zonecode,String extension) {
		super();

		this.pwd 			= pwd;
		this.name 			= name;
		this.position 		= position;
		this.department 	= department;
		this.mobile 		= mobile;
		this.homenumber 	= homenumber;
		this.mailaddress 	= mailaddress;
		this.authmode 		= authmode;
		this.username 		= username;
		this.password 		= password;
		this.ipaddress 		= ipaddress;
		this.ipport 		= ipport;
		this.zonecode 		= zonecode;
		this.extension		= extension;
	}

	public IpcsDetailDTO(String pwd,String name,String position,int department,String mobile,String homenumber,String mailaddress,int authmode,String username,String password,String ipaddress,int ipport,String zonecode,String extension,String beforePassword) {
		super();

		this.pwd 			= pwd;
		this.name 			= name;
		this.position 		= position;
		this.department 	= department;
		this.mobile 		= mobile;
		this.homenumber 	= homenumber;
		this.mailaddress 	= mailaddress;
		this.authmode 		= authmode;
		this.username 		= username;
		this.password 		= password;
		this.ipaddress 		= ipaddress;
		this.ipport 		= ipport;
		this.zonecode 		= zonecode;
		this.extension		= extension;
		this.beforePassword	= beforePassword;
	}

	// 비밀번호
	public String getPwd() {
		return pwd;
	}
	public void setPwd(String pwd) {
		this.pwd = pwd;
	}

	// 이름
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}

	// 직급
	public String getPosition() {
		return position;
	}
	public void setPosition(String position) {
		this.position = position;
	}

	// 부서
	public int getDepartment() {
		return department;
	}
	public void setDepartment(int department) {
		this.department = department;
	}

	// 핸드폰
	public String getMobile() {
		return mobile;
	}
	public void setMobile(String mobile) {
		this.mobile = mobile;
	}

	// 집전화
	public String getHomeNumber() {
		return homenumber;
	}
	public void setHomeNumber(String homenumber) {
		this.homenumber = homenumber;
	}

	// 메일주소
	public String getMailaddress() {
		return mailaddress;
	}
	public void setMailAddress(String mailaddress) {
		this.mailaddress = mailaddress;
	}

	// 내선번호
	public String getExtension() {
		return extension;
	}
	public void setExtension(String extension) {
		this.extension = extension;
	}
	
	
	public int getAuthmode() {
		return authmode;
	}
	public void setAuthmode(int authmode) {
		this.authmode = authmode;
	}

	public String getUsername() {
		return username;
	}
	public void setUsername(String username) {
		this.username = username;
	}
	
	public String getPassword() {
		return password;
	}
	public void setPassword(String password) {
		this.password = password;
	}
	
	public String getIpaddress() {
		return ipaddress;
	}
	public void setIpaddress(String ipaddress) {
		this.ipaddress = ipaddress;
	}
	
	public int getIpport() {
		return ipport;
	}
	public void setIpport(int ipport) {
		this.ipport = ipport;
	}

	public String getZonecode() {
		return zonecode;
	}
	public void setZonecode(String zonecode) {
		this.zonecode = zonecode;
	}
	
	// 헥사 변경전 인증비밀번호
	public String getBeforePassword() {
		return beforePassword;
	}
	public void setBeforePassword(String beforePassword) {
		this.beforePassword = beforePassword;
	}
	
}
