package dto;

public class AdditionDTO {
	
	private String callerservice     = "";
	private String answerservice     = "";
	private String commonservice     = "";
	private String virtualcid     	= "";
	
	public AdditionDTO () {}

	public String getCallerService() {
		return callerservice;
	}
	public void setCallerService(String callerservice) {
		this.callerservice = callerservice;
	}

	public String getAnswerService() {
		return answerservice;
	}
	public void setAnswerService(String answerservice) {
		this.answerservice = answerservice;
	}

	public String getCommonService() {
		return commonservice;
	}
	public void setCommonService(String commonservice) {
		this.commonservice = commonservice;
	}

	public String getVirtualCid() {
		return virtualcid;
	}
	public void setVirtualCid(String virtualcid) {
		this.virtualcid = virtualcid;
	}
	
}
