	public String toXmlString(String parent, String blank) {


       String xml = blank+"            <Buddy " ;
       xml += " parent=\"" + getDeptName() + "\" " ; 
       xml += " description=\"" + getPosition()+ " " +getName() + "\" " ; 
       xml += " KeyNumber=\"" + getPhoneNum() + "\" " ; 
       xml += " officebox=\"" + getKeyNumber() + "\" operation=\"0\" presence=\"1\">\n" ; 

       xml += blank+"                <ID>" + getID().substring(0,getID().lastIndexOf("@")) + "</ID>\n" ;
       xml += blank+"                <NickName>" + getName() + "</NickName>\n" ;
       xml += blank+"                <Position>" + getPosition() + "</Position>\n" ;
       xml += blank+"                <Department>" + getDeptName() + "</Department>\n" ;
       xml += blank+"                <Mobile>" + getMobile() + "</Mobile>\n" ;
       xml += blank+"                <Extension>" + getExtension() + "</Extension>\n" ;
       xml += blank+"                <MailAddress>" + getMailAddress() + "</MailAddress>\n" ;
       xml += blank+"                <CompanyNumber>" + getPhoneNum() + "</CompanyNumber>\n" ;
       xml += blank+"                <HomeNumber>" + getHomeNumber() + "</HomeNumber>\n" ;
