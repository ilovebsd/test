package bizportal.nasacti.ipivr.common;

public class IpivrConfig {
	public String strRealPath = "";                     // Servlet 절대 경로
	public String strAnCodePath = "";                   // Ancode 파일 경로
	public String strFileMentPath = "";                 // Filement 파일 경로
	public int maxUploadSize = 10;                      // 최대 업로드 사이즈
	public String strResponseModeKey = "";              // Normal IPCS의 초기 응답모드 키값
	public String strKeyActionVisibleType = "2";        // Normal IPCS의 음성안내번호 관리의 초기 응답모드 설정에서의 Key Action을 보여주는 정도를 설정한다.  -  1: 일부 보여줌(Moniter Transfer, Goto Code, Undefined), 2: 전부 보여줌

	private static IpivrConfig instance = new IpivrConfig();

	public static IpivrConfig getInstance() {
		return instance;
	}

	private IpivrConfig() {
	}
}
