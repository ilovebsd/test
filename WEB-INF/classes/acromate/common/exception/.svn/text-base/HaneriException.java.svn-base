package acromate.common.exception;

public class HaneriException extends Exception
{
    private String m_sMsgCode;
	
    public HaneriException() {}

    public HaneriException (String sMsgCode)
    {
        super(sMsgCode);
        m_sMsgCode = sMsgCode;
    }

    public String getMessageCode() {
    	return m_sMsgCode;
    }	
}
