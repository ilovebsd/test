package command;

import javax.servlet.http.HttpServletRequest;
import javax.servlet.http.HttpServletResponse;
import com.acromate.util.Str;
import system.SystemConfigFileMake;

public class QualityServicesInsertCommand implements Command {
	public String execute(HttpServletRequest req, HttpServletResponse res) {
		String 	forwardingPage 	= "";				
		String 	resStr 			= "";
		
		try {
			String 	strType 	= new String(Str.CheckNullString(req.getParameter("hiType")).getBytes("8859_1"), "euc-kr");
			String 	strMb 		= new String(Str.CheckNullString(req.getParameter("hiMb")).getBytes("8859_1"), "euc-kr");
			String 	strQuality 	= new String(Str.CheckNullString(req.getParameter("hiQuality")).getBytes("8859_1"), "euc-kr");
			String 	strIndex 	= new String(Str.CheckNullString(req.getParameter("hiIndex")).getBytes("8859_1"), "euc-kr");
			String 	strSchedule = new String(Str.CheckNullString(req.getParameter("hiSchedule")).getBytes("8859_1"), "euc-kr");
			String 	strUse 		= new String(Str.CheckNullString(req.getParameter("hiUse")).getBytes("8859_1"), "euc-kr");
			
			resStr = "qualityServices.jsp";
			
			SystemConfigFileMake makeFile = new SystemConfigFileMake();
			int insertResult = makeFile.QualityServicesFile(strType, strMb, strQuality, strIndex, strSchedule, strUse);
			
			if(insertResult==1){
				resStr = "qualityServices.jsp";
				res.sendRedirect("/bizportal/system/"+resStr);
			}else{
				res.sendRedirect("/bizportal/errorMessage.jsp");
			}

		} catch (Exception e) {
			try{
				res.sendRedirect("/bizportal/errorMessage.jsp");
			} catch (Exception ee) {
			}
			e.printStackTrace();
		} finally {

		}		
		return forwardingPage;
	}
	
}
