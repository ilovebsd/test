package command;

import javax.servlet.http.HttpServletRequest;
import javax.servlet.http.HttpServletResponse;

import business.LogHistory;

import com.acromate.util.Str;
import system.SystemConfigFileMake;

public class AclInsertNewCommand implements Command {
	public String execute(HttpServletRequest req, HttpServletResponse res) {
		String 	forwardingPage 	= "";				
		String 	resStr 			= "";
		
		try {
			String 	strAccessType 	= new String(Str.CheckNullString(req.getParameter("hiAccessType")).getBytes("8859_1"), "euc-kr");
			String 	strProtocol 	= new String(Str.CheckNullString(req.getParameter("hiProtocol")).getBytes("8859_1"), "euc-kr");
			String 	strIpType 		= new String(Str.CheckNullString(req.getParameter("hiIpType")).getBytes("8859_1"), "euc-kr");
			String 	strIp 			= new String(Str.CheckNullString(req.getParameter("hiIp")).getBytes("8859_1"), "euc-kr");
			String 	strPortType 	= new String(Str.CheckNullString(req.getParameter("hiPortType")).getBytes("8859_1"), "euc-kr");
			String 	strPort 		= new String(Str.CheckNullString(req.getParameter("hiPort")).getBytes("8859_1"), "euc-kr");
			
			String 	userID			= new String(req.getParameter("hiUserID").getBytes("8859_1"), "euc-kr");		// 肺弊牢 ID
			
			resStr = "acl_New.jsp?startType=0";
			
			SystemConfigFileMake makeFile = new SystemConfigFileMake();
			int insertResult = makeFile.aclFile_New(strAccessType, strProtocol, strIpType, strIp, strPortType, strPort);
			
			if(insertResult==1){
				// ############### LogHistory 贸府  ###############
				String		strIp2		= req.getRemoteAddr();
				LogHistory	logHistory 	= new LogHistory();
				if("any".equals(strIpType)){
					int int_result = logHistory.LogHistorySave(userID+"|90|ACL ("+strAccessType+" / 葛电 IP林家)|1|"+strIp2);
				}else{
					int int_result = logHistory.LogHistorySave(userID+"|90|ACL ("+strAccessType+" / "+strIp+")|1|"+strIp2);
				}
				// ##############################################

				res.sendRedirect("/bizportal/system/"+resStr);
			}else{
				res.sendRedirect("/bizportal/errorMessage.jsp");
			}

		} catch (Exception e) {
			try{
				res.sendRedirect("/bizportal/errorMessage.jsp");
			} catch (Exception ee) {
			}
			e.printStackTrace();
		} finally {

		}		
		return forwardingPage;
	}
	
}
