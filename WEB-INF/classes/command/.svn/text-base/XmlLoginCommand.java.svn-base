package command;

import java.io.*;
import java.net.*;
import system.SystemConfigSet;
import acromate.common.StaticString;
		
public class XmlLoginCommand {
	private SystemConfigSet systemConfig 	= new SystemConfigSet();
	
    public void execute() throws IOException { 
        URL url = null; 

        try{
        	String 	osType 			= "";
        	String 	urlPath 		= "";
        	
            osType = systemConfig.osChk();
            if("win".equals(osType)){
            	urlPath = "http://localhost:8080"+StaticString.ContextRoot+"/mainLogin.do";		// Å×½ºÆ®
            }else{
            	urlPath = "http://localhost"+StaticString.ContextRoot+"/mainLogin.do";
            }
        	
             //url = new URL("http://localhost:8080/bizportal/mainLogin.do"); 
             url = new URL(urlPath);
             
        }catch(MalformedURLException e){ 
             e.printStackTrace(); 
        } 
        BufferedInputStream in = new BufferedInputStream(url.openConnection().getInputStream()); 
		  in.close();
   }
	
}
