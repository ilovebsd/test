package command;

import javax.servlet.http.HttpServletRequest;
import javax.servlet.http.HttpServletResponse;
import business.LogHistory;
import com.acromate.util.Str;
import dao.gateway.GatewayDAO;

public class GatewaySlotChangeCommand implements Command {
	public String execute(HttpServletRequest req, HttpServletResponse res) {
		String 	forwardingPage 	= "";
		boolean returnVal 		= false;
		
		try {			
			String 	beforeSlot	= new String(Str.CheckNullString(req.getParameter("hiBeforeSlot")).getBytes("8859_1"), "euc-kr");
			String 	afterSlot	= new String(Str.CheckNullString(req.getParameter("hiAfterSlot")).getBytes("8859_1"), "euc-kr");
			
			String 	userID		= new String(req.getParameter("hiUserID").getBytes("8859_1"), "euc-kr");		// 로그인 ID
			
			GatewayDAO dao 	= new GatewayDAO();

			returnVal = dao.slotUpdate(beforeSlot, afterSlot);
						
			if (returnVal) {
				// ############### LogHistory 처리  ###############
				String		strIp		= req.getRemoteAddr();
				LogHistory	logHistory 	= new LogHistory();
				int int_result = logHistory.LogHistorySave(userID+"|87|채널 등록("+beforeSlot+"번 Slot을 "+afterSlot+"번 Slot 으로 변경)|1|"+strIp);
				// ##############################################
				
				System.out.println("Gateway Channel 저장 성공 .......");
				res.sendRedirect("/bizportal/newGateway/channelSet.jsp");
			}else{
				System.out.println("Gateway Channel 저장 .......");
			}			
		
		} catch (Exception e) {

			e.printStackTrace();
		} finally {
			
			
		}
		return forwardingPage;
	}
}
