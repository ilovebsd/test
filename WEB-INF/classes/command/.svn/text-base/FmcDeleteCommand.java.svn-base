package command;

import javax.servlet.http.HttpServletRequest;
import javax.servlet.http.HttpServletResponse;

import com.acromate.util.Str;
import dao.useconfig.AddServiceDAO;

import java.io.File;

public class  FmcDeleteCommand implements Command {
	public String execute(HttpServletRequest req, HttpServletResponse res) {
		String 	forwardingPage 	= "";
		boolean returnVal 		= false;
		File    tempFile        = null;  // 임시 파일 객체
//		String 	deleteStr  		= "";
		
		try {			
			String 	deleteStr 	= new String(Str.CheckNullString(req.getParameter("deleteStr")).getBytes("8859_1"), "euc-kr");		// 
			
			String 	userID		= new String(req.getParameter("hiUserID").getBytes("8859_1"), "euc-kr");		// 로그인 ID
			
			//System.out.println("########## deleteStr : "+deleteStr);
			
			AddServiceDAO dao 	= new AddServiceDAO();
			
			// ############### LogHistory 처리  ###############
			//returnVal = dao.fmcDelete(deleteStr);
			returnVal = dao.fmcDelete(deleteStr, req.getRemoteAddr(), userID);
			// ##############################################
			
			if (returnVal) {
				res.sendRedirect("/bizportal/useconfig/addServiceFmcList.jsp");
			}else{
				res.sendRedirect("/bizportal/errorMessage.jsp");
			}			
		
		} catch (Exception e) {
			try{
				res.sendRedirect("/bizportal/errorMessage.jsp");
			} catch (Exception ee) {
			}
			e.printStackTrace();
		} finally {
			
			
		}
		return forwardingPage;
	}
}
