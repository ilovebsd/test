import business.LogHistory;
	
public class  UserMRBTDeleteCommand implements Command {
	public String execute(HttpServletRequest req, HttpServletResponse res) {
		String 	forwardingPage 	= "";
		boolean returnVal 		= false;
//		File    tempFile        = null;  // 임시 파일 객체
//		String 	deleteStr  		= "";
		
		try {			
			String 	e164 		= new String(Str.CheckNullString(req.getParameter("d_Ei64")).getBytes("8859_1"), "euc-kr");		// 
			String 	dayValue 	= new String(Str.CheckNullString(req.getParameter("d_DayValue")).getBytes("8859_1"), "euc-kr");		//
			String 	startTime 	= new String(Str.CheckNullString(req.getParameter("d_StartTime")).getBytes("8859_1"), "euc-kr");		//
			String 	endTime 	= new String(Str.CheckNullString(req.getParameter("d_EndTime")).getBytes("8859_1"), "euc-kr");		//
			String 	fileName 	= new String(Str.CheckNullString(req.getParameter("d_filename")).getBytes("8859_1"), "euc-kr");		//
			
			String 	userID		= new String(req.getParameter("hiUserID").getBytes("8859_1"), "euc-kr");		// 로그인 ID
			
			//System.out.println("########## deleteStr : "+deleteStr);
			
			AddServiceDAO dao 	= new AddServiceDAO();
			returnVal = dao.e164UserMRBTDelete(e164, dayValue, startTime, endTime, fileName);

			if (returnVal) {
				// ############### LogHistory 처리  #############
				String		strIp		= req.getRemoteAddr();
				LogHistory	logHistory 	= new LogHistory();
				int int_result = logHistory.LogHistorySave(userID+"|83|일반통화 연결음 파일삭제 ("+e164+" 번)|2|"+strIp);
				// ##############################################

				res.sendRedirect("/bizportal/useconfig/addServiceNewMRBTList2.jsp?viewType="+e164);
			}else{
				res.sendRedirect("/bizportal/errorMessage.jsp");
			}			
		
		} catch (Exception e) {
			try{
				res.sendRedirect("/bizportal/errorMessage.jsp");
			} catch (Exception ee) {
			}
			e.printStackTrace();
		} finally {
			
			
		}
		return forwardingPage;
	}
}
