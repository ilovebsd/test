import business.LogHistory;

public class  CallChangeEditCommand implements Command {
	public String execute(HttpServletRequest req, HttpServletResponse res) {
		String 	forwardingPage 	= "";
		boolean returnVal 		= false;
//		String 	deleteStr  		= "";
		
		try {			
			String 	e164 			= new String(Str.CheckNullString(req.getParameter("hiE164")).getBytes("8859_1"), "euc-kr");			// 
			String 	changeNum 		= new String(Str.CheckNullString(req.getParameter("hiNumber")).getBytes("8859_1"), "euc-kr");		// 
			String 	changeType 		= new String(Str.CheckNullString(req.getParameter("hiChangeType")).getBytes("8859_1"), "euc-kr");	// 1 : 일반, 2 : VMS
			String 	oldChangeNum 	= new String(Str.CheckNullString(req.getParameter("hiOldNumber")).getBytes("8859_1"), "euc-kr");
			
			String 	userID			= new String(req.getParameter("hiUserID").getBytes("8859_1"), "euc-kr");		// 로그인 ID
			
			//System.out.println("########## deleteStr : "+deleteStr);
			
			AddServiceDAO dao 	= new AddServiceDAO();
			//returnVal = dao.callChangeNumberEdit(e164, changeNum, changeType, oldChangeNum);
			returnVal = dao.callChangeNumberEdit2(e164, changeNum, changeType);
			
			if (returnVal) {
				// ############### LogHistory 처리  #############
				String		strIp		= req.getRemoteAddr();
				LogHistory	logHistory 	= new LogHistory();
				int int_result = logHistory.LogHistorySave(userID+"|83|대표번호 무조건 착신전화 ("+e164+" 번)|3|"+strIp);
				// #############################################

				res.sendRedirect("/bizportal/useconfig/addServiceCallChangeList.jsp");
			}else{
				res.sendRedirect("/bizportal/errorMessage.jsp");
			}			
		
		} catch (Exception e) {
			try{
				res.sendRedirect("/bizportal/errorMessage.jsp");
			} catch (Exception ee) {
			}
			e.printStackTrace();
		} finally {
			
			
		}
		return forwardingPage;
	}
}
