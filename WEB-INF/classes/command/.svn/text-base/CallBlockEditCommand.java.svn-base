package command;

import javax.servlet.http.HttpServletRequest;
import javax.servlet.http.HttpServletResponse;
import javax.servlet.http.HttpSession;

import com.acromate.util.Str;
import dao.useconfig.AddServiceDAO;

import acromate.common.StaticString;
import org.apache.commons.fileupload.*;
import java.io.*;
import java.util.*;
import java.security.*;
import sun.misc.*;

public class  CallBlockEditCommand implements Command {
	public String execute(HttpServletRequest req, HttpServletResponse res) {
		String 	forwardingPage 	= "";
		boolean returnVal 		= false;
//		String 	deleteStr  		= "";
		
		try {			
			String 	callBlockType 	= new String(Str.CheckNullString(req.getParameter("hiCallBlockType")).getBytes("8859_1"), "euc-kr");		//
			String 	e164 			= new String(Str.CheckNullString(req.getParameter("hiE164")).getBytes("8859_1"), "euc-kr");		// 
			String 	prefixType 		= new String(Str.CheckNullString(req.getParameter("prefixType")).getBytes("8859_1"), "euc-kr");
			String 	blockType 		= new String(Str.CheckNullString(req.getParameter("blockType")).getBytes("8859_1"), "euc-kr");
			String 	blockE164 		= new String(Str.CheckNullString(req.getParameter("blockE164")).getBytes("8859_1"), "euc-kr");
			String 	note 			= new String(Str.CheckNullString(req.getParameter("note")).getBytes("8859_1"), "euc-kr");
			
			//System.out.println("########## deleteStr : "+deleteStr);
			
			AddServiceDAO dao 	= new AddServiceDAO();
			returnVal = dao.callBlockEdit(e164, callBlockType, prefixType, blockType, blockE164, note);
			
			if (returnVal) {
				res.sendRedirect("/bizportal/useconfig/addServiceCallBlockList.jsp?viewType="+e164);
				//forwardingPage = "/bizportal/useconfig/addServiceCallBlockList.jsp?viewType="+e164;
			}else{
				res.sendRedirect("/bizportal/errorMessage.jsp");
			}			
		
		} catch (Exception e) {
			try{
				res.sendRedirect("/bizportal/errorMessage.jsp");
			} catch (Exception ee) {
			}
			e.printStackTrace();
		} finally {
			
			
		}
		return forwardingPage;
	}
}
