public class BuddyDeleteCommand implements Command {
	public String execute(HttpServletRequest req, HttpServletResponse res) {
		String forwardingPage = "";
        boolean returnVal 	= false;
        
		try {
			String 	deleteStr 	= new String(Str.CheckNullString(req.getParameter("deleteStr")).getBytes("8859_1"), "euc-kr");		// 작성자ID
			
			String 	userID		= new String(req.getParameter("hiUserID").getBytes("8859_1"), "euc-kr");		// 로그인 ID
			
			// 사용자 정보 수정
			BuddyDAO dao = new BuddyDAO();
			
			// ############### LogHistory 처리  ###############
			//returnVal = dao.buddyDelete(deleteStr);
			returnVal = dao.buddyDelete(deleteStr, req.getRemoteAddr(), userID);
			// ##############################################
			
			if (returnVal) {
				System.out.println("조직도 개인정보 삭제 성공 .......");
				//forwardingPage = "/bizportal/publicAddrList.jsp?group=" + nGroupId + "&result=" + Integer.toString(updateResult);
				res.sendRedirect("/bizportal/buddy/buddyList.jsp");
			}else{
				System.out.println("조직도 개인정보 삭제 실패 .......");
				//forwardingPage = "/address/publicGroupList.jsp?session=0";
			}
			
		} catch (Exception e) {
			System.out.println("조직도 개인정보 에러발생 .......");
			//forwardingPage = "/address/publicGroupList.jsp?group=" + nGroupId + "&result=-1" ;
			
			e.printStackTrace();
		}		
		return forwardingPage;
	}
}
