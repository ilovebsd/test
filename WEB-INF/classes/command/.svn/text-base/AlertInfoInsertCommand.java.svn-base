package command;

import javax.servlet.http.HttpServletRequest;
import javax.servlet.http.HttpServletResponse;
import javax.servlet.http.HttpSession;

import com.acromate.util.Str;
import dao.useconfig.AddServiceDAO;

import acromate.common.StaticString;
import org.apache.commons.fileupload.*;
import java.io.*;
import java.util.*;
import java.security.*;
import sun.misc.*;

public class  AlertInfoInsertCommand implements Command {
	public String execute(HttpServletRequest req, HttpServletResponse res) {
		String 	forwardingPage 	= "";
		boolean returnVal 		= false;
//		String 	deleteStr  		= "";
		
		try {			
			String 	insertStr 		= new String(Str.CheckNullString(req.getParameter("insertStr")).getBytes("8859_1"), "euc-kr");		// 
			String 	alertIntoType 	= new String(Str.CheckNullString(req.getParameter("hiAlertIntoType")).getBytes("8859_1"), "euc-kr");		//
			
			String 	userID			= new String(req.getParameter("hiUserID").getBytes("8859_1"), "euc-kr");		// 로그인 ID
			
			//System.out.println("########## deleteStr : "+deleteStr);
			
			AddServiceDAO dao 	= new AddServiceDAO();
			
			// ############### LogHistory 처리  ###############
			//returnVal = dao.alertInfoInsert(insertStr, alertIntoType);
			returnVal = dao.alertInfoInsert(insertStr, alertIntoType, req.getRemoteAddr(), userID);
			// ##############################################
			
			if (returnVal) {
				res.sendRedirect("/bizportal/useconfig/addServiceAlertInfoList.jsp");
			}else{
				res.sendRedirect("/bizportal/errorMessage.jsp");
			}			
		
		} catch (Exception e) {
			try{
				res.sendRedirect("/bizportal/errorMessage.jsp");
			} catch (Exception ee) {
			}
			e.printStackTrace();
		} finally {
			
			
		}
		return forwardingPage;
	}
}
