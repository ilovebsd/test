package command;

import javax.servlet.http.HttpServletRequest;
import javax.servlet.http.HttpServletResponse;

import business.LogHistory;

import com.acromate.util.Str;
import system.SystemConfigFileMake;

public class PortForward_HybridInsertCommand implements Command {
	public String execute(HttpServletRequest req, HttpServletResponse res) {
		String 	forwardingPage 	= "";				
		String 	resStr 			= "";
		
		try {
			String 	strProtocol = new String(Str.CheckNullString(req.getParameter("hiProtocol")).getBytes("8859_1"), "euc-kr");
			String 	strInport 	= new String(Str.CheckNullString(req.getParameter("hiInport")).getBytes("8859_1"), "euc-kr");
			String 	strIp 		= new String(Str.CheckNullString(req.getParameter("hiIp")).getBytes("8859_1"), "euc-kr");
			String 	strOutport 	= new String(Str.CheckNullString(req.getParameter("hiOutport")).getBytes("8859_1"), "euc-kr");

			String 	userID		= new String(req.getParameter("hiUserID").getBytes("8859_1"), "euc-kr");		// 로그인 ID
			
			resStr = "PortForwarding_Hybrid.jsp";
			
			SystemConfigFileMake makeFile = new SystemConfigFileMake();
			int insertResult = makeFile.portForwardingFile_Hybrid(strProtocol, strInport, strIp, strOutport);
			
			if(insertResult==1){
				// ############### LogHistory 처리  ###############
				String		strIp2		= req.getRemoteAddr();
				LogHistory	logHistory 	= new LogHistory();
				int int_result = logHistory.LogHistorySave(userID+"|86|포트 포워딩 설정("+strIp+")|1|"+strIp2);
				// ##############################################

				resStr = "PortForwarding_Hybrid.jsp";
				res.sendRedirect("/bizportal/system/"+resStr);
			}else{
				res.sendRedirect("/bizportal/errorMessage.jsp");
			}

		} catch (Exception e) {
			try{
				res.sendRedirect("/bizportal/errorMessage.jsp");
			} catch (Exception ee) {
			}
			e.printStackTrace();
		} finally {

		}		
		return forwardingPage;
	}
	
}
