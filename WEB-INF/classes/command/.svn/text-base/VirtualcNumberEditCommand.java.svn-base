package command;

import javax.servlet.http.HttpServletRequest;
import javax.servlet.http.HttpServletResponse;
import javax.servlet.http.HttpSession;

import com.acromate.util.Str;
import dao.useconfig.AddServiceDAO;

import acromate.common.StaticString;
import org.apache.commons.fileupload.*;
import java.io.*;
import java.util.*;
import java.security.*;
import sun.misc.*;

import business.LogHistory;

public class  VirtualcNumberEditCommand implements Command {
	public String execute(HttpServletRequest req, HttpServletResponse res) {
		String 	forwardingPage 	= "";
		boolean returnVal 		= false;
//		String 	deleteStr  		= "";
		
		try {			
			String 	e164 			= new String(Str.CheckNullString(req.getParameter("hiE164")).getBytes("8859_1"), "euc-kr");				// 
			String 	displayNumber 	= new String(Str.CheckNullString(req.getParameter("hiDisplayNumber")).getBytes("8859_1"), "euc-kr");	// 가상발신 번호(Display)
			String 	fromNumber 		= new String(Str.CheckNullString(req.getParameter("hiFromNumber")).getBytes("8859_1"), "euc-kr");		// 가상발신 번호(From)
			
			String 	userID			= new String(req.getParameter("hiUserID").getBytes("8859_1"), "euc-kr");		// 로그인 ID
			
			String	userparam	= "3,";
			if("".equals(displayNumber)){
				userparam = userparam+" ,";
			}else{
				userparam = userparam+displayNumber+",";
			}
			if("".equals(fromNumber)){
				userparam = userparam+" ";
			}else{
				userparam = userparam+fromNumber;
			}

			//System.out.println("########## deleteStr : "+deleteStr);
			
			AddServiceDAO dao 	= new AddServiceDAO();
			returnVal = dao.virtualNumberEdit(e164, userparam);
			
			if (returnVal) {
				// ############### LogHistory 처리  ###############
				String		strIp		= req.getRemoteAddr();
				LogHistory	logHistory 	= new LogHistory();
				int int_result = logHistory.LogHistorySave(userID+"|83|가상발신번호 ("+e164+" 번)|3|"+strIp);
				// ##############################################

				res.sendRedirect("/bizportal/useconfig/addServiceVirtualList.jsp");
			}else{
				res.sendRedirect("/bizportal/errorMessage.jsp");
			}			
		
		} catch (Exception e) {
			try{
				res.sendRedirect("/bizportal/errorMessage.jsp");
			} catch (Exception ee) {
			}
			e.printStackTrace();
		} finally {
			
			
		}
		return forwardingPage;
	}
}
