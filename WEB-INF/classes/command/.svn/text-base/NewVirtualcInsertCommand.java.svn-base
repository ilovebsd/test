package command;

import javax.servlet.http.HttpServletRequest;
import javax.servlet.http.HttpServletResponse;
import javax.servlet.http.HttpSession;

import com.acromate.util.Str;
import dao.useconfig.AddServiceDAO;

public class NewVirtualcInsertCommand implements Command {
	public String execute(HttpServletRequest req, HttpServletResponse res) {
		String 	forwardingPage 	= "";
		boolean returnVal 		= false;

		try {			
			String 	virtualcType1	= new String(Str.CheckNullString(req.getParameter("hiVirtualc1")).getBytes("8859_1"), "euc-kr");		// 가상발신 유형1
			String 	virtualcType2	= new String(Str.CheckNullString(req.getParameter("hiVirtualc2")).getBytes("8859_1"), "euc-kr");		// 가상발신 유형2
			String 	displayNumber	= new String(Str.CheckNullString(req.getParameter("hiDisplayNumber")).getBytes("8859_1"), "euc-kr");	// 가상발신 번호(Display)
			String 	fromNumber		= new String(Str.CheckNullString(req.getParameter("hiFromNumber")).getBytes("8859_1"), "euc-kr");		// 가상발신 번호(From)
			String 	ei64			= new String(Str.CheckNullString(req.getParameter("hiEi64")).getBytes("8859_1"), "euc-kr");				// 
			
//			if("".equals(displayNumber)){
//				System.out.println("##### virtualcType1 : 스페이스 체크 성공");
//			}else{
//				System.out.println("##### virtualcType1 : 스페이스 체크 실패");
//			}
			
			AddServiceDAO dao 	= new AddServiceDAO();
			returnVal = dao.newVirtualcNumberSave(virtualcType1, virtualcType2, displayNumber, fromNumber, ei64);
			
			if (returnVal) {
				//System.out.println("가상발신 E164 등록 성공 .......");
				res.sendRedirect("/bizportal/useconfig/addServiceList3.jsp");
			}else{
				//System.out.println("가상발신 E164 등록 실패 .......");
				res.sendRedirect("/bizportal/errorMessage.jsp");
			}			
		
		} catch (Exception e) {
			try{
				res.sendRedirect("/bizportal/errorMessage.jsp");
			} catch (Exception ee) {
			}
			e.printStackTrace();
		} finally {
			
			
		}
		return forwardingPage;
	}
}
