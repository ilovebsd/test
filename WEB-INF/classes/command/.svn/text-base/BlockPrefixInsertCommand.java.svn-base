public class BlockPrefixInsertCommand implements Command {
	public String execute(HttpServletRequest req, HttpServletResponse res) {
		String 	forwardingPage 	= "";
		boolean returnVal 		= false;

		try {			
			String 	e164		= new String(Str.CheckNullString(req.getParameter("hiEi64")).getBytes("8859_1"), "euc-kr");			// e164
			String 	blockType	= new String(Str.CheckNullString(req.getParameter("hiBlockType")).getBytes("8859_1"), "euc-kr");	// 착신제한 유형

			BlockPrefixDAO dao 	= new BlockPrefixDAO();
			returnVal = dao.e164Update(e164, blockType);

			if (returnVal) {
				System.out.println("착신제한 E164 수정 성공 .......");
				//forwardingPage = "/bizportal/publicAddrList.jsp?group=" + nGroupId + "&result=" + Integer.toString(updateResult);				
				res.sendRedirect("/bizportal/addition/blockPrefixList.jsp");
				//res.sendRedirect("/bizportal/buddy/buddyList.jsp?h_DeptID="+hiDept);
			}else{
				System.out.println("착신제한 E164 수정 실패 .......");
				//forwardingPage = "/address/publicGroupList.jsp?session=0";
			}			
		
		} catch (Exception e) {

			e.printStackTrace();
		} finally {
			
			
		}
		return forwardingPage;
	}
}
