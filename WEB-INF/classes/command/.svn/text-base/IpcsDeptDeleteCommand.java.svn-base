import dao.ipcs.IpcsDeptDAO;

public class IpcsDeptDeleteCommand implements Command {
	public String execute(HttpServletRequest req, HttpServletResponse res) {
		String forwardingPage = "";

        int 	nGroupId 	= 0;
        boolean returnVal 	= false;
        try {
			String 	hiKeynumberid	= new String(req.getParameter("hiKeynumberid").getBytes("8859_1"), "euc-kr");	// 부서대표번호
			
			IpcsDeptDAO dao = new IpcsDeptDAO();
			
			// 사용자 정보만  삭제
			returnVal = dao.ipcsDeptDelete(hiKeynumberid);

			if (returnVal) {
				System.out.println("IPCS 삭제 성공 .......");
				//forwardingPage = "/bizportal/publicAddrList.jsp?group=" + nGroupId + "&result=" + Integer.toString(updateResult);
				res.sendRedirect("/bizportal/ipcs/ipcsDeptNumberList.jsp");
			}else{
				System.out.println("IPCS 삭제 실패 .......");
				forwardingPage = "/bizportal/errorMessage.jsp";
			}
		} catch (Exception e) {
			System.out.println("IPCS 에러발생 .......");
			forwardingPage = "/bizportal/errorMessage.jsp";
			
			e.printStackTrace();
		}		
		return forwardingPage;
	}
}
