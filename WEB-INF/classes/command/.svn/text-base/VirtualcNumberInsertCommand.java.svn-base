package command;

import javax.servlet.http.HttpServletRequest;
import javax.servlet.http.HttpServletResponse;
import javax.servlet.http.HttpSession;

import com.acromate.util.Str;
import dao.useconfig.AddServiceDAO;

public class VirtualcNumberInsertCommand implements Command {
	public String execute(HttpServletRequest req, HttpServletResponse res) {
		String 	forwardingPage 	= "";
		boolean returnVal 		= false;

		try {			
			String 	insertStr 		= new String(Str.CheckNullString(req.getParameter("insertStr")).getBytes("8859_1"), "euc-kr");			// 
			String 	displayNumber 	= new String(Str.CheckNullString(req.getParameter("hiDisplayNumber")).getBytes("8859_1"), "euc-kr");	// 가상발신 번호(Display)
			String 	fromNumber 		= new String(Str.CheckNullString(req.getParameter("hiFromNumber")).getBytes("8859_1"), "euc-kr");		// 가상발신 번호(From)
			
			String 	userID			= new String(req.getParameter("hiUserID").getBytes("8859_1"), "euc-kr");		// 로그인 ID
			
			String	userparam	= "3,";
			if("".equals(displayNumber)){
				userparam = userparam+" ,";
			}else{
				userparam = userparam+displayNumber+",";
			}
			if("".equals(fromNumber)){
				userparam = userparam+" ";
			}else{
				userparam = userparam+fromNumber;
			}
			
			AddServiceDAO dao 	= new AddServiceDAO();
			
			// ############### LogHistory 처리  ###############
			//returnVal = dao.virtualNumberInsert(insertStr, userparam);
			returnVal = dao.virtualNumberInsert(insertStr, userparam, req.getRemoteAddr(), userID);
			// ##############################################
			
			if (returnVal) {
				//System.out.println("가상발신 E164 등록 성공 .......");
				res.sendRedirect("/bizportal/useconfig/addServiceVirtualList.jsp");
			}else{
				//System.out.println("가상발신 E164 등록 실패 .......");
				res.sendRedirect("/bizportal/errorMessage.jsp");
			}			
		
		} catch (Exception e) {
			try{
				res.sendRedirect("/bizportal/errorMessage.jsp");
			} catch (Exception ee) {
			}
			e.printStackTrace();
		} finally {
			
			
		}
		return forwardingPage;
	}
}
