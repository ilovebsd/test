package command;

import javax.servlet.http.HttpServletRequest;
import javax.servlet.http.HttpServletResponse;
import javax.servlet.http.HttpSession;

import com.acromate.util.Str;
import dao.useconfig.AddServiceDAO;

public class SendInsertCommand implements Command {
	public String execute(HttpServletRequest req, HttpServletResponse res) {
		String 	forwardingPage 	= "";
		boolean returnVal 		= false;

		try {			
			String 	blockType	= new String(Str.CheckNullString(req.getParameter("hiBlockType1")).getBytes("8859_1"), "euc-kr");	// 발신제한 유형
			String 	blockNumber	= new String(Str.CheckNullString(req.getParameter("hiBlockNumber")).getBytes("8859_1"), "euc-kr");	// 발신제한 번호
			String 	description	= new String(Str.CheckNullString(req.getParameter("hiDescription")).getBytes("8859_1"), "euc-kr");	// 발신제한 번호 설명

			AddServiceDAO dao 	= new AddServiceDAO();
			returnVal = dao.sendNumberSave(blockNumber, description, blockType);

			if (returnVal) {
				System.out.println("발신제한 E164 등록 성공 .......");
				//forwardingPage = "/bizportal/publicAddrList.jsp?group=" + nGroupId + "&result=" + Integer.toString(updateResult);				
				res.sendRedirect("/bizportal/useconfig/addServiceList.jsp");
			}else{
				System.out.println("발신제한 E164 등록 실패 .......");
				//forwardingPage = "/address/publicGroupList.jsp?session=0";
				res.sendRedirect("/bizportal/errorMessage.jsp");
			}			
		
		} catch (Exception e) {
			try{
				res.sendRedirect("/bizportal/errorMessage.jsp");
			} catch (Exception ee) {
			}
			e.printStackTrace();
		} finally {
			
			
		}
		return forwardingPage;
	}
}
