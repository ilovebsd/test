package command;

import javax.servlet.http.HttpServletRequest;
import javax.servlet.http.HttpServletResponse;
import javax.servlet.http.HttpSession;

import acromate.ConnectionManager;
import acromate.common.StaticString;
import business.LogHistory;

import com.acromate.driver.db.DataStatement;
import com.acromate.util.Str;
import waf.BaseEntity;
import waf.SessionManager;
import webuser.ServerLogin;
import dto.system.PrefixTableIdDTO;
import dao.system.PrefixTableIdDAO;

public class NewPrefixIdSaveCommand implements Command {
	public String execute(HttpServletRequest req, HttpServletResponse res) {
		DataStatement 	stmt 			= null;
		String 			forwardingPage 	= "";				
		String 			resStr 			= "";
		try {
			String 	prefixid 		= new String(Str.CheckNullString(req.getParameter("hiRoutingID")).getBytes("8859_1"), "euc-kr");		//
			String 	tabletype 		= new String(Str.CheckNullString(req.getParameter("hiTableType")).getBytes("8859_1"), "euc-kr");		// 
			String 	description		= new String(Str.CheckNullString(req.getParameter("hiDescription")).getBytes("8859_1"), "euc-kr");		//
			String 	hiRootID		= new String(Str.CheckNullString(req.getParameter("hiRootID")).getBytes("8859_1"), "euc-kr");			//
			String 	hiPrefixType	= new String(Str.CheckNullString(req.getParameter("hiPrefixType")).getBytes("8859_1"), "euc-kr");		//
			String 	hiSpecialChk	= new String(Str.CheckNullString(req.getParameter("hiSpecialChk")).getBytes("8859_1"), "euc-kr");		//
			
			String 	hiRouteSelectRule	= new String(Str.CheckNullString(req.getParameter("hiRouteSelectRule")).getBytes("8859_1"), "euc-kr");		//
			
			String 	userID			= new String(req.getParameter("hiUserID").getBytes("8859_1"), "euc-kr");		// 로그인 ID
			
			resStr = "routingList.jsp";

			SessionManager manager = SessionManager.getInstance();
			if (manager.isLogin(req)) {
				HttpSession hs = req.getSession();
				String id = hs.getId();
				BaseEntity entity = manager.getBaseEntity(id);	
				
				PrefixTableIdDTO dto = new PrefixTableIdDTO();
				dto.setPrefixTableId(prefixid);
				dto.setTableType(Integer.parseInt(tabletype));
				dto.setDescription(description);
				
				stmt = ConnectionManager.allocStatement("SSW");
								
				PrefixTableIdDAO dao = new PrefixTableIdDAO();
				int insertResult = dao.newInsert(stmt, dto, hiRootID, hiPrefixType, Str.CheckNullInt(hiRouteSelectRule), hiSpecialChk);
				
				// ############### LogHistory 처리  ###############
				String		strIp		= req.getRemoteAddr();
				LogHistory	logHistory 	= new LogHistory();
				int int_result = logHistory.LogHistorySave(userID+"|86|번호정책별 라우팅 ("+prefixid+")|1|"+strIp);
				// ##############################################

				res.sendRedirect("/bizportal/system/"+resStr);
			} 
			else
				res.sendRedirect("/bizportal/system/"+resStr+"?session=0");
		
		} catch (Exception e) {
			try{
				res.sendRedirect("/bizportal/errorMessage.jsp");
			} catch (Exception ee) {
			}
			e.printStackTrace();
		} finally {
			//할당받은 DataStatement 객체는 반납
			if (stmt != null ) ConnectionManager.freeStatement(stmt);
		}		
		return forwardingPage;
	}
}
